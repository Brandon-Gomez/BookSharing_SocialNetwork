{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport apiClient from \"@/services/ApiService\";\nexport default {\n  components: {},\n  data() {\n    return {\n      isCurrentUser: false,\n      countPosts: 0,\n      userForm: {\n        id: \"\",\n        name: \"\",\n        description: \"\",\n        username: \"\",\n        password: \"\",\n        gender: \"\"\n      },\n      originalUsername: \"\",\n      // <-- Agrega esto\n      profilePhoto: null,\n      profileImageFile: null,\n      publications: 0,\n      follows: {\n        isFollowing: false,\n        followers: 0,\n        following: 0\n      }\n    };\n  },\n  mounted() {\n    this.fetchUser();\n    this.fetchPosts();\n  },\n  methods: {\n    handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.profileImageFile = file;\n        this.profilePhoto = URL.createObjectURL(file);\n      }\n    },\n    async fetchUser() {\n      try {\n        const token = localStorage.getItem(\"authToken\");\n        const username = this.$route.params.username;\n        const response = await apiClient.get(`/profile/${username}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.userForm = {\n          id: response.data.user.id,\n          name: response.data.user.name,\n          username: response.data.user.username,\n          description: response.data.user.description || \"\",\n          gender: response.data.user.gender || \"\",\n          profile_picture: response.data.user.profile_picture\n        };\n        this.originalUsername = response.data.user.username; // <-- Guarda el username original\n        this.isCurrentUser = response.data.isCurrentUser;\n      } catch (error) {\n        console.error(\"Error al obtener el usuario:\", error.response?.data || error);\n        alert(\"Error al obtener los datos del usuario.\");\n      }\n    },\n    async updateUser() {\n      const token = localStorage.getItem(\"authToken\");\n      if (token) {\n        try {\n          // Copia el userForm para no modificar el original\n          const userData = {\n            ...this.userForm\n          };\n          // Si el username no cambió, elimínalo del objeto a enviar\n          if (userData.username === this.originalUsername) {\n            delete userData.username;\n          }\n          await apiClient.put(`/profile/${this.userForm.id}`, userData, {\n            headers: {\n              Authorization: `Bearer ${token}`\n            }\n          });\n          if (this.profileImageFile) {\n            const formData = new FormData();\n            formData.append(\"photo\", this.profileImageFile);\n            await apiClient.post(`/upload/profile`, formData, {\n              headers: {\n                Authorization: `Bearer ${token}`,\n                \"Content-Type\": \"multipart/form-data\"\n              }\n            });\n          }\n        } catch (error) {\n          console.error(\"Error updating profile:\", error);\n        }\n      }\n    },\n    async fetchPosts() {\n      try {\n        alert(\"Fetching posts...\");\n        // Usa el userId desde userForm\n        const userId = this.userForm.id;\n        alert(\"User ID:\", userId);\n        if (!userId) return; // Espera a que fetchUser cargue el id\n\n        const res = await apiClient.get(`/posts/count/${userId}`);\n        this.countPosts = res.data.count;\n        console.log(\"Publicaciones:\", res);\n      } catch (error) {\n        console.error(\"Error fetching posts:\", error);\n      }\n    },\n    getUserImage() {\n      return this.userForm.profile_picture || \"https://firebasestorage.googleapis.com/v0/b/booksharing-socialnetwork.appspot.com/o/profile%2Fdefault.jpg?alt=media\";\n    },\n    logout() {\n      localStorage.removeItem(\"authToken\");\n      localStorage.removeItem(\"profileData\");\n      this.$router.push(\"/login\");\n    }\n  }\n};","map":{"version":3,"names":["apiClient","components","data","isCurrentUser","countPosts","userForm","id","name","description","username","password","gender","originalUsername","profilePhoto","profileImageFile","publications","follows","isFollowing","followers","following","mounted","fetchUser","fetchPosts","methods","handleFileUpload","event","file","target","files","URL","createObjectURL","token","localStorage","getItem","$route","params","response","get","headers","Authorization","user","profile_picture","error","console","alert","updateUser","userData","put","formData","FormData","append","post","userId","res","count","log","getUserImage","logout","removeItem","$router","push"],"sources":["D:\\Downloads\\BookSharing_SocialNetwork\\fronvue\\src\\components\\editprofile_component.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"page-title-overlap bg-dark pt-4\">\r\n      <div\r\n        class=\"container d-flex flex-wrap flex-sm-nowrap justify-content-center justify-content-sm-between align-items-center pt-2\"\r\n      >\r\n        <div class=\"d-flex align-items-center pb-3\">\r\n          <div\r\n            class=\"rounded-circle position-relative flex-shrink-0\"\r\n            style=\"width: 6.375rem\"\r\n          >\r\n            <img\r\n              class=\"rounded-circle\"\r\n              :src=\"userForm.profile_picture\"\r\n              alt=\"Createx Studio\"\r\n            />\r\n          </div>\r\n          <div class=\"ps-3\">\r\n            <h3 class=\"text-light fs-lg mb-0\">{{ userForm.name }}</h3>\r\n            <span class=\"d-block text-light fs-ms opacity-60 py-1\"\r\n              >@{{ userForm.username }}</span\r\n            >\r\n          </div>\r\n        </div>\r\n        <!-- Desktop version -->\r\n        <div class=\"d-none d-sm-flex\">\r\n          <div class=\
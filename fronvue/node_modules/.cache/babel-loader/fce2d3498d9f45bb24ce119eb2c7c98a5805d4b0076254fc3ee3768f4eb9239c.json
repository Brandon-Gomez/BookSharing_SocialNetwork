{"ast":null,"code":"import apiClient from '@/services/ApiService'; // Asegúrate de que está correctamente configurado\n\nexport default {\n  data() {\n    return {\n      searchQuery: '',\n      users: [] // Lista de usuarios encontrados\n    };\n  },\n  methods: {\n    async searchUser() {\n      if (this.searchQuery.length > 2) {\n        // Inicia búsqueda si hay más de 2 caracteres\n        console.log('Iniciando búsqueda para:', this.searchQuery); // Log del query\n\n        try {\n          const response = await apiClient.get('/search', {\n            params: {\n              query: this.searchQuery\n            }\n          });\n          console.log('Datos recibidos desde el backend:', response.data); // Verifica que los datos lleguen correctamente\n          this.users = response.data; // Asigna los resultados de la búsqueda\n        } catch (error) {\n          console.error('Error al buscar usuarios:', error); // Log del error en caso de fallo\n        }\n      } else {\n        console.log('Búsqueda muy corta, no se realizará ninguna consulta.'); // Log para cuando la búsqueda es muy corta\n        this.users = []; // Limpia la lista si la búsqueda es corta\n      }\n    }\n  }\n};","map":{"version":3,"names":["apiClient","data","searchQuery","users","methods","searchUser","length","console","log","response","get","params","query","error"],"sources":["C:\\DATA\\POW\\BookSharing_SocialNetwork\\fronvue\\src\\components\\search_component.vue"],"sourcesContent":["<template>\r\n  <div class=\"search-bar\">\r\n    <input\r\n      type=\"text\"\r\n      v-model=\"searchQuery\"\r\n      @input=\"searchUser\"\r\n      placeholder=\"Buscar usuario...\"\r\n      class=\"form-control\"\r\n    />\r\n    <ul v-if=\"users.length\" class=\"list-group mt-2\">\r\n      <li v-for=\"user in users\" :key=\"user.id\" class=\"list-group-item\">\r\n        {{ user.username }} ({{ user.name }})\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport apiClient from '@/services/ApiService'; // Asegúrate de que está correctamente configurado\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      users: [],  // Lista de usuarios encontrados\r\n    };\r\n  },\r\n  methods: {\r\n  async searchUser() {\r\n    if (this.searchQuery.length > 2) {  // Inicia búsqueda si hay más de 2 caracteres\r\n      console.log('Iniciando búsqueda para:', this.searchQuery);  // Log del query\r\n\r\n      try {\r\n        const response = await apiClient.get('/search', { params: { query: this.searchQuery } });\r\n        console.log('Datos recibidos desde el backend:', response.data);  // Verifica que los datos lleguen correctamente\r\n        this.users = response.data;  // Asigna los resultados de la búsqueda\r\n      } catch (error) {\r\n        console.error('Error al buscar usuarios:', error);  // Log del error en caso de fallo\r\n      }\r\n    } else {\r\n      console.log('Búsqueda muy corta, no se realizará ninguna consulta.');  // Log para cuando la búsqueda es muy corta\r\n      this.users = [];  // Limpia la lista si la búsqueda es corta\r\n    }\r\n  }\r\n}\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.search-bar {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.form-control {\r\n  width: 100%;\r\n  padding: 10px;\r\n}\r\n\r\n.list-group {\r\n  margin-top: 10px;\r\n}\r\n\r\n.list-group-item {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":"AAkBA,OAAOA,SAAQ,MAAO,uBAAuB,EAAE;;AAE/C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,EAAE,CAAG;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACT,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI,IAAI,CAACH,WAAW,CAACI,MAAK,GAAI,CAAC,EAAE;QAAG;QAClCC,OAAO,CAACC,GAAG,CAAC,0BAA0B,EAAE,IAAI,CAACN,WAAW,CAAC,EAAG;;QAE5D,IAAI;UACF,MAAMO,QAAO,GAAI,MAAMT,SAAS,CAACU,GAAG,CAAC,SAAS,EAAE;YAAEC,MAAM,EAAE;cAAEC,KAAK,EAAE,IAAI,CAACV;YAAY;UAAE,CAAC,CAAC;UACxFK,OAAO,CAACC,GAAG,CAAC,mCAAmC,EAAEC,QAAQ,CAACR,IAAI,CAAC,EAAG;UAClE,IAAI,CAACE,KAAI,GAAIM,QAAQ,CAACR,IAAI,EAAG;
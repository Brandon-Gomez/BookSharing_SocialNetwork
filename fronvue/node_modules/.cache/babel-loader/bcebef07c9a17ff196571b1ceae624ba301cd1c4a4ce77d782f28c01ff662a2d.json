{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      profileData: this.getProfileData(),\n      defaultPicture: 'favicon.png' // Ruta a una imagen por defecto\n    };\n  },\n  methods: {\n    getProfileData() {\n      return JSON.parse(localStorage.getItem('profileData')) || {\n        name: '',\n        username: '',\n        profile_picture: ''\n      };\n    },\n    updateProfileData() {\n      this.profileData = this.getProfileData();\n    },\n    navigateToHome() {\n      this.$router.push('/');\n    },\n    navigateToProfile() {\n      const isAuthenticated = this.checkAuthentication();\n      if (isAuthenticated && this.profileData.username) {\n        this.$router.push(`/profile/${this.profileData.username}`);\n      } else {\n        alert(\"Por favor, inicia sesión primero.\");\n      }\n    },\n    navigateToSearch() {\n      this.$router.push('/search');\n    },\n    logout() {\n      localStorage.removeItem('authToken');\n      localStorage.removeItem('profileData');\n      this.profileData = {\n        name: '',\n        username: '',\n        profile_picture: ''\n      }; // Resetear el perfil aquí\n      this.$router.push('/login');\n    },\n    checkAuthentication() {\n      return !!localStorage.getItem('authToken');\n    }\n  },\n  mounted() {\n    window.addEventListener('storage', this.updateProfileData);\n  },\n  beforeDestroy() {\n    window.removeEventListener('storage', this.updateProfileData);\n  }\n};","map":{"version":3,"names":["data","profileData","getProfileData","defaultPicture","methods","JSON","parse","localStorage","getItem","name","username","profile_picture","updateProfileData","navigateToHome","$router","push","navigateToProfile","isAuthenticated","checkAuthentication","alert","navigateToSearch","logout","removeItem","mounted","window","addEventListener","beforeDestroy","removeEventListener"],"sources":["C:\\DATA\\POW\\BookSharing_SocialNetwork\\fronvue\\src\\compone
{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport apiClient from '@/services/ApiService';\nexport default {\n  data() {\n    return {\n      profileData: JSON.parse(localStorage.getItem('profileData')) || {\n        name: '',\n        username: '',\n        profile_picture: ''\n      },\n      isSidebarOpen: false,\n      // Estado de la barra lateral\n      isSearchVisible: false,\n      searchQuery: '',\n      users: []\n    };\n  },\n  methods: {\n    getProfileImage() {\n      const defaultImageUrl = \"https://firebasestorage.googleapis.com/v0/b/booksharing-socialnetwork.appspot.com/o/profile%2Fdefault.jpg?alt=media\"; // Reemplaza con la URL de tu imagen predeterminada en Firebase\n      return this.profileData.profile_picture ? this.profileData.profile_picture : defaultImageUrl;\n    },\n    navigateToHome() {\n      this.$router.push('/');\n    },\n    navigateToProfile() {\n      const isAuthenticated = this.checkAuthentication();\n      if (isAuthenticated) {\n        this.$router.push(`/profile/${this.profileData.username}`);\n      } else {\n        alert(\"Por favor, inicia sesión primero.\");\n        this.$router.push('/login');\n      }\n    },\n    toggleSearch() {\n      this.isSearchVisible = !this.isSearchVisible;\n    },\n    toggleSidebar() {\n      this.isSidebarOpen = !this.isSidebarOpen; // Alternar la visibilidad de la barra lateral\n    },\n    async searchUser() {\n      if (this.searchQuery.length > 2) {\n        try {\n          const response = await apiClient.get('/search', {\n            params: {\n              query: this.searchQuery\n            }\n          });\n          this.users = response.data;\n        } catch (error) {\n          console.error('Error al buscar usuarios:', error);\n        }\n      } else {\n        this.users = [];\n      }\n    },\n    goToProfile(username) {\n      this.$router.push(`/profile/${username}`);\n      this.isSearchVisible = false;\n    },\n    logout() {\n      localStorage.removeItem('authToken');\n      localStorage.removeItem('profileData');\n      this.$router.push('/login').then(() => {\n        window.location.reload();\n      });\n    },\n    checkAuthentication() {\n      return !!localStorage.getItem('authToken');\n    }\n  }\n};","map":{"version":3,"names":["apiClient","data","profileData","JSON","parse","localStorage","getItem","name","username","profile_picture","isSidebarOpen","isSearchVisible","searchQuery","users","methods","getProfileImage","defaultImageUrl","navigateToHome","$router","push","navigateToProfile","isAuthenticated","checkAuthentication","alert","toggleSearch","toggleSidebar","searchUser","length","response","get","params","query","error","console","goToProfile","logout","removeItem","then","window","location","reload"],"sources":["D:\\Downloads\\BookSharing_SocialNetwork\\fronvue\\src\\components\\navbar_component.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- Botón para alternar la barra lateral -->\r\n    <button @click=\"toggleSidebar\" class=\"toggle-button\">\r\n      ☰ <!-- Icono de hamburguesa -->\r\n    </button>\r\n\r\n    <div class=\"sidebar\" :class=\"{ open: isSidebarOpen }\">\r\n      <div class=\"profile-section text-center py-4\">\r\n        <img\r\n          :src=\"getProfileImage()\"\r\n          class=\"profile-picture rounded-circle mb-3\"\r\n          alt=\"Profile\"\r\n          v-if=\"profileData.username\"\r\n        />\r\n        <h5 class=\"text-dark\">{{ profileData.name }}</h5>\r\n        <p class=\"text-muted\">{{ profileData.username || '' }}</p>\r\n      </div>\r\n      \r\n      <ul class=\"list-group list-group-flush\">\r\n        <li class=\"list-group-item list-group-item-action\" @click=\"navigateToHome\">\r\n          <i class=\"fa fa-home\"></i> Inicio\r\n        </li>\r\n        <li class=\"list-group-item list-group-item-action\" @click=\"navigateToProfile\">\r\n          <i class=\"fa fa-user\"></i> Perfil\r\n        </li>\r\n        <li class=\"list-group-item list-group-item-action\" @click=\"logout\">\r\n          <i class=\"fa fa-sign-out\"></i> Cerrar sesión\r\n        </li>\r\n        <li class=\"list-group-item list-group-item-action\" @click=\"toggleSearch\">\r\n          <i class=\"fa fa-search\"></i> Buscar\r\n        </li>\r\n      </ul>\r\n\r\n      <div v-if=\"isSearchVisible\" class=\"search-bar p-3\">\r\n        <input\r\n          type=\"text\"\r\n          v-model=\"searchQuery\"\r\n          @input=\"searchUser\"\r\n          placeholder=\"Buscar usuario...\"\r\n          class=\"form-control\"\r\n        />\r\n        <ul v-if=\"users.length\" class=\"list-group mt-2\">\r\n          <li \r\n            v-for=\"user in users\" \r\n            :key=\"user.id\" \r\n            class=\"list-group-item list-group-item-action\" \r\n            @click=\"goToProfile(user.username)\">\r\n            {{ user.username }} ({{ user.name }})\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport apiClient from '@/services/ApiService';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      profileData: JSON.parse(localStorage.getItem('profileData')) || {\r\n        name: '',\r\n        username: '',\r\n        profile_picture: '',\r\n      },\r\n      isSidebarOpen: false, // Estado de la barra lateral\r\n      isSearchVisible: false,\r\n      searchQuery: '',\r\n      users: [],\r\n    };\r\n  },\r\n  methods: {\r\n    getProfileImage() {\r\n    const defaultImageUrl = \"https://firebasestorage.googleapis.com/v0/b/booksharing-socialnetwork.appspot.com/o/profile%2Fdefault.jpg?alt=media\"; // Reemplaza con la URL de tu imagen predeterminada en Firebase\r\n    return this.profileData.profile_picture\r\n      ? this.profileData.profile_picture\r\n      : defaultImageUrl;\r\n  },\r\n    navigateToHome() {\r\n      this.$router.push('/');\r\n    },\r\n    navigateToProfile() {\r\n      const isAuthenticated = this.checkAuthentication();\r\n      if (isAuthenticated) {\r\n        this.$router.push(`/profile/${this.profileData.username}`);\r\n      } else {\r\n        alert(\"Por favor, inicia sesión primero.\");\r\n        this.$router.push('/login');\r\n      }\r\n    },\r\n    toggleSearch() {\r\n      this.isSearchVisible = !this.isSearchVisible;\r\n    },\r\n    toggleSidebar() {\r\n      this.isSidebarOpen = !this.isSidebarOpen; // Alternar la visibilidad de la barra lateral\r\n    },\r\n    async searchUser() {\r\n      if (this.searchQuery.length > 2) {\r\n        try {\r\n          const response = await apiClient.get('/search', { params: { query: this.searchQuery } });\r\n          this.users = response.data;\r\n        } catch (error) {\r\n          console.error('Error al buscar usuarios:', error);\r\n        }\r\n      } else {\r\n        this.users = [];\r\n      }\r\n    },\r\n    goToProfile(username) {\r\n      this.$router.push(`/profile/${username}`);\r\n      this.isSearchVisible = false;\r\n    },\r\n    logout() {\r\n      localStorage.removeItem('authToken');\r\n      localStorage.removeItem('profileData');\r\n      this.$router.push('/login').then(() => {\r\n        window.location.reload();\r\n      });\r\n    },\r\n    checkAuthentication() {\r\n      return !!localStorage.getItem('authToken');\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* Sidebar general style */\r\n\r\n</style>\r\n"],"mappings":";AAyDA,OAAOA,SAAQ,MAAO,uBAAuB;AAE7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,aAAa,CAAC,KAAK;QAC9DC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,eAAe,EAAE;MACnB,CAAC;MACDC,aAAa,EAAE,KAAK;MAAE;MACtBC,eAAe,EAAE,KAAK;MACtBC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAClB,MAAMC,eAAc,GAAI,qHAAqH,EAAE;MAC/I,OAAO,IAAI,CAACd,WAAW,CAACO,eAAc,GAClC,IAAI,CAACP,WAAW,CAACO,eAAc,GAC/BO,eAAe;IACrB,CAAC;IACCC,cAAcA,CAAA,EAAG;MACf,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;IACxB,CAAC;IACDC
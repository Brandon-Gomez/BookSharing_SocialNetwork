{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport apiClient from '../services/ApiService';\nimport MainLayout from '../layouts/MainLayout.vue';\nexport default {\n  components: {\n    MainLayout\n  },\n  data() {\n    return {\n      email: '2@gmail.com',\n      password: '2'\n      // codigo: '',\n    };\n  },\n  methods: {\n    async loginUser() {\n      try {\n        const response = await apiClient.post('/login', {\n          email: this.email,\n          password: this.password\n          // codigo: this.codigo\n        });\n        // Almacenar el token o redirigir al usuario\n\n        const username = response.data.username;\n        const is_admin = response.data.is_admin;\n        localStorage.setItem('authToken', response.data.token);\n        localStorage.setItem('isAdmin', is_admin);\n        localStorage.setItem('username', response.data.username);\n        if (is_admin) {\n          this.$router.push(`/admin/dashboard`).then(() => {\n            window.location.reload();\n          });\n        } else {\n          this.$router.push(`/profile/${username}`).then(() => {\n            window.location.reload();\n          });\n        }\n      } catch (error) {\n        console.error('Error en el login:', error.response);\n      }\n    }\n  }\n};","map":{"version":3,"names":["apiClient","MainLayout","components","data","email","password","methods","loginUser","response","post","username","is_admin","localStorage","setItem","token","$router","push","then","window","location","reload","error","console"],"sources":["D:\\Downloads\\BookSharing_SocialNetwork\\fronvue\\src\\components\\login_component.vue"],"sourcesContent":["<template>\r\n  <MainLayout>\r\n   <div class=\"container\">\r\n          <div class=\"row flex-center min-vh-100 py-5\">\r\n            <div class=\"col-sm-10 col-md-8 col-lg-5 col-xl-5 col-xxl-3\"><a class=\"d-flex flex-center text-decoration-none mb-4\" href=\"../../../index.html\">\r\n                <div class=\
{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport apiClient from '../services/ApiService';\nexport default {\n  components: {},\n  data() {\n    return {\n      email: '2@gmail.com',\n      password: '2'\n      // codigo: '',\n    };\n  },\n  methods: {\n    async loginUser() {\n      try {\n        const response = await apiClient.post('/login', {\n          email: this.email,\n          password: this.password\n          // codigo: this.codigo\n        });\n        // Almacenar el token o redirigir al usuario\n\n        const username = response.data.username;\n        const is_admin = response.data.is_admin;\n        localStorage.setItem('authToken', response.data.token);\n        localStorage.setItem('isAdmin', is_admin);\n        localStorage.setItem('username', response.data.username);\n        if (is_admin) {\n          this.$router.push(`/admin/dashboard`).then(() => {\n            window.location.reload();\n          });\n        } else {\n          this.$router.push(`/profile/${username}`).then(() => {\n            window.location.reload();\n          });\n        }\n      } catch (error) {\n        console.error('Error en el login:', error.response);\n      }\n    }\n  }\n};","map":{"version":3,"names":["apiClient","components","data","email","password","methods","loginUser","response","post","username","is_admin","localStorage","setItem","token","$router","push","then","window","location","reload","error","console"],"sources":["D:\\Downloads\\BookSharing_SocialNetwork\\fronvue\\src\\components\\login_component.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <main class=\"main\" id=\"top\">\r\n      <div class=\"container-fluid px-0\">\r\n        <div class=\"row vh-100 g-0\">\r\n          <div class=\"col-lg-6 position-relative d-none d-lg-block\">\r\n            <div class=\"bg-holder\" style=\"background-image:url(../../../assets/img/bg/32.png);\">\r\n            </div>\r\n            <!--/.bg-holder-->\r\n\r\n          </d
{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport apiClient from '@/services/ApiService';\nexport default {\n  components: {},\n  data() {\n    return {\n      isCurrentUser: false,\n      posts: [],\n      userData: {\n        name: '',\n        username: '',\n        email: '',\n        birthdate: '',\n        description: '',\n        user_picture: ''\n      },\n      profilePhoto: null,\n      profileImageFile: null,\n      publications: 0,\n      follows: {\n        isFollowing: false,\n        followers: 0,\n        following: 0\n      }\n    };\n  },\n  mounted() {\n    this.fetchUser();\n    this.fetchPosts();\n  },\n  methods: {\n    handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.profileImageFile = file;\n        this.profilePhoto = URL.createObjectURL(file);\n      }\n    },\n    async fetchUser() {\n      try {\n        const token = localStorage.getItem(\"authToken\");\n        const username = this.$route.params.username;\n        const response = await apiClient.get(`/profile/${username}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log(\"User data:\", response.data);\n        this.userData = response.data.user;\n        this.isCurrentUser = response.data.isCurrentUser;\n      } catch (error) {\n        console.error(\"Error al obtener el usuario:\", error.response?.data || error);\n        alert(\"Error al obtener los datos del usuario.\");\n      }\n    },\n    async updateUser() {\n      const token = localStorage.getItem('authToken');\n      if (token) {\n        try {\n          console.log('Updating user data:', this.userData);\n          // const response = await apiClient.put(`/profile/${this.userData.username}/edit-account`, this.userData, {\n          //   headers: {\n          //     Authorization: `Bearer ${token}`\n          //   }\n          // });\n          // console.log('User updated:', response.data);\n\n          if (this.profileImageFile) {\n            const formData = new FormData();\n            formData.append('photo', this.profileImageFile);\n            await apiClient.post(`/upload/profile`, formData, {\n              headers: {\n                Authorization: `Bearer ${token}`,\n                'Content-Type': 'multipart/form-data'\n              }\n            });\n          }\n          this.$router.push(`/profile/${this.userData.username}/edit - account`).then(() => {\n            window.location.reload();\n          });\n        } catch (error) {\n          console.error('Error updating profile:', error);\n        }\n      }\n    },\n    async fetchPosts() {\n      try {\n        const token = localStorage.getItem(\"authToken\");\n        const username = this.$route.params.username;\n        const res = await apiClient.get(`/profile/${username}/posts?page=${this.page}&limit=${this.postsPerPage}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.posts = res.data.posts;\n      } catch (error) {\n        console.error(\"Error fetching posts:\", error);\n      }\n    },\n    getUserImage() {\n      return this.userData.profile_picture || \"https://firebasestorage.googleapis.com/v0/b/booksharing-socialnetwork.appspot.com/o/profile%2Fdefault.jpg?alt=media\";\n    }\n  }\n};","map":{"version":3,"names":["apiClient","components","data","isCurrentUser","posts","userData","name","username","email","birthdate","description","user_picture","profilePhoto","profileImageFile","publications","follows","isFollowing","followers","following","mounted","fetchUser","fetchPosts","methods","handleFileUpload","event","file","target","files","URL","createObjectURL","token","localStorage","getItem","$route","params","response","get","headers","Authorization","console","log","user","error","alert","updateUser","formData","FormData","append","post","$router","push","then","window","location","reload","res","page","postsPerPage","getUserImage","profile_picture"],"sources":["D:\\Downloads\\BookSharing_SocialNetwork\\fronvue\\src\\components\\editprofile_component.vue"],"sourcesContent":["<template>\r\n\r\n  <div>\r\n    <div class=\"page-title-overlap bg-dark pt-4\">\r\n      <div\r\n        class=\"container d-flex flex-wrap flex-sm-nowrap justify-content-center justify-content-sm-between align-items-center pt-2\">\r\n        <div class=\"d-flex align-items-center pb-3\">\r\n          <div class=\"rounded-circle position-relative flex-shrink-0\" style=\"width: 6.375rem\">\r\n            <img class=\"rounded-circle\" :src=\"userData.profile_picture\" alt=\"Createx Studio\" />\r\n          </div>\r\n          <div class=\"ps-3\">\r\n            <h3 class=\"text-light fs-lg mb-0\">{{ userData.name }}</h3>\r\n            <span class=\"d-block text-light fs-ms opacity-60 py-1\">@{{ userData.username }}</span>\r\n          </div>\r\n        </div>\r\n        <!-- Desktop version -->\r\n        <div class=\"d-none d-sm-flex\">\r\n          <div class=\"text-sm-end me-4\">\r\n            <div class=\"text-light fs-base\">\r\n              {{ posts.length }}\r\n              <span class=\"text-muted fs-base pl-1\">Publicaciones</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-sm-end me-4\">\r\n            <div class=\"text-light fs-base\">\r\n              {{ follows.followers\r\n              }}<span class=\"text-muted fs-base pl-1\">Seguidores</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-sm-end\">\r\n            <div class=\"text-light fs-base\">\r\n              {{ follows.following\r\n              }}<span class=\"text-muted fs-base pl-1\">Seguidos</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- Mobile version -->\r\n        <div class=\"d-flex d-sm-none w-100 justify-content-around my-3\">\r\n          <div class=\"text-center\">\r\n            <div class=\"text-light fs-6 fw-bold\">{{ posts.length }}</div>\r\n            <div class=\"text-muted fs-xs\">Publicaciones</div>\r\n          </div>\r\n          <div class=\"text-center\">\r\n            <div class=\"text-light fs-6 fw-bold\">{{ follows.followers }}</div>\r\n            <div class=\"text-muted fs-xs\">Seguidores</div>\r\n          </div>\r\n          <div class=\"text-center\">\r\n            <div class=\"text-light fs-6 fw-bold\">{{ follows.following }}</div>\r\n            <div class=\"text-muted fs-xs\">Seguidos</div>\r\n          </div>\r\n        </div>\r\n\r\n\r\n      </div>\r\n    </div>\r\n    <div class=\"container mb-5 pb-3\">\r\n      <div class=\"bg-light shadow-lg rounded-3 overflow-hidden\">\r\n        <div class=\"row\">\r\n          <aside class=\"col-lg-4 pt-4 pt-lg-0 pe-xl-5\">\r\n            <div class=\"d-block d-lg-none p-4\"><a class=\"btn btn-outline-accent d-block\" href=\"#account-menu\"\r\n                data-bs-toggle=\"collapse\" aria-expanded=\"true\"><i class=\"ci-menu me-2\"></i>Account menu</a></div>\r\n            <div class=\"bg-white rounded-3 shadow-lg pt-1  mb-lg-0\">\r\n              <div class=\"d-lg-block collapse\" id=\"account-menu\">\r\n                <div class=\"bg-secondary px-4 py-3\">\r\n                  <h3 class=\"fs-sm mb-0 text-muted\">Cuenta</h3>\r\n                </div>\r\n                <ul class=\"list-unstyled mb-0\">\r\n                  <li class=\"border-bottom mb-0\">\r\n                    <a class=\"nav-link-style d-flex align-items-center px-4 py-3 \" href=\"#\">\r\n                      <i class=\"ci-book opacity-60 me-2\"></i>\r\n                      Publicaciones\r\n                    </a>\r\n                  </li>\r\n                  <li class=\"border-bottom mb-0\">\r\n                    <a class=\"nav-link-style d-flex align-items-center px-4 py-3\" href=\"#\">\r\n                      <i class=\"ci-heart opacity-60 me-2\"></i>\r\n                      Favoritos\r\n                    </a>\r\n                  </li>\r\n                </ul>\r\n                <div v-if=\"isCurrentUser\">\r\n                  <div class=\"bg-secondary px-4 py-3\">\r\n                    <h3 class=\"fs-sm mb-0 text-muted\">Configuración</h3>\r\n                  </div>\r\n                  <ul class=\"list-unstyled mb-0\">\r\n                    <li class=\"border-bottom mb-0\">\r\n                      <a type=\"button\" class=\"nav-link-style d-flex align-items-center px-4 py-3 active\"\r\n                        @click.prevent=\"goToEditProfile\">\r\n                        <i class=\"ci-user opacity-60 me-2\"></i>\r\n                        Información personal\r\n                      </a>\r\n                    </li>\r\n                    <li class=\"d-lg-none border-top mb-0\">\r\n                      <a class=\"nav-link-style d-flex align-items-center px-4 py-3\" href=\"#\" @click=\"logout\">\r\n                        <i class=\"ci-sign-out opacity-60 me-2\"></i>\r\n                        Cerrar sesión\r\n                      </a>\r\n                    </li>\r\n                  </ul>\r\n                </div>\r\n\r\n              </div>\r\n            </div>\r\n          </aside>\r\n          <section class=\"col-lg-8 pt-lg-4 pb-4 mb-3\">\r\n            <h2 class=\"h3 pt-2 pb-4 mb-0 text-center text-sm-start border-bottom\">Modificar perfil</h2>\r\n            <div class=\"tab-content mt-4\">\r\n              <div class=\"tab-pane fade show active\" id=\"profile\" role=\"tabpanel\">\r\n                <div class=\"bg-secondary rounded-3 p-4 mb-4\">\r\n                  <div class=\"d-flex align-items-center\">\r\n                    <img class=\"rounded\" :src=\"getUserImage()\" width=\"90\" alt=\"Foto de perfil\">\r\n                    <div class=\"ps-3\">\r\n                      <input type=\"file\" @change=\"handleFileUpload\" class=\"form-control mt-2\" accept=\"image/*\">\r\n                      <div class=\"p mb-0 fs-ms text-muted\">\r\n                        Sube una imagen JPG, GIF o PNG. 300 x 300 recomendado.\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"row gx-4 gy-3\">\r\n                  <div class=\"col-sm-6\">\r\n                    <label class=\"form-label\" for=\"dashboard-fn\">First Name</label>\r\n                    <input class=\"form-control\" type=\"text\" id=\"dashboard-fn\" v-model=\"userData.name\">\r\n                  </div>\r\n                  <div class=\"col-sm-6\">\r\n                    <label class=\"form-label\" for=\"dashboard-ln\">Last Name</label>\r\n                    <input class=\"form-control\" type=\"text\" id=\"dashboard-ln\" v-model=\"userData.username\">\r\n                  </div>\r\n                  <div class=\"col-sm-6\">\r\n                    <label class=\"form-label\" for=\"dashboard-email\">Email address</label>\r\n                    <input class=\"form-control\" type=\"text\" id=\"dashboard-email\" v-model=\"userData.email\" disabled=\"\">\r\n                  </div>\r\n                  <div class=\"col-sm-6\">\r\n                    <label class=\"form-label\" for=\"dashboard-profile-name\">Profile Name</label>\r\n                    <input class=\"form-control\" type=\"text\" id=\"dashboard-profile-name\" v-model=\"userData.description\">\r\n                  </div>\r\n                  <div class=\"col-sm-6\">\r\n                    <label class=\"form-label\" for=\"dashboard-country\">Country</label>\r\n                    <select class=\"form-select\" id=\"dashboard-country\">\r\n                      <option value=\"\">Select country</option>\r\n                      <option value=\"Argentina\">Argentina</option>\r\n                      <option value=\"Belgium\">Belgium</option>\r\n                      <option value=\"France\">France</option>\r\n                      <option value=\"Germany\">Germany</option>\r\n                      <option value=\"Madagascar\" selected=\"\">Madagascar</option>\r\n                      <option value=\"Spain\">Spain</option>\r\n                      <option value=\"UK\">United Kingdom</option>\r\n                      <option value=\"USA\">USA</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"col-sm-6\">\r\n                    <label class=\"form-label\" for=\"dashboard-city\">City</label>\r\n                    <input class=\"form-control\" type=\"text\" id=\"dashboard-city\">\r\n                  </div>\r\n                  <div class=\"col-sm-6\">\r\n                    <label class=\"form-label\" for=\"dashboard-address\">Address Line</label>\r\n                    <input class=\"form-control\" type=\"text\" id=\"dashboard-address\">\r\n                  </div>\r\n                  <div class=\"col-sm-6\">\r\n                    <label class=\"form-label\" for=\"dashboard-zip\">ZIP Code</label>\r\n                    <input class=\"form-control\" type=\"text\" id=\"dashboard-zip\">\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <hr class=\"mt-2 mb-4\">\r\n                    <div class=\"d-sm-flex justify-content-between align-items-center\">\r\n                      <div class=\"form-check\">\r\n                        <input class=\"form-check-input\" type=\"checkbox\" id=\"freelancer\" checked=\"\">\r\n                        <label class=\"form-check-label\" for=\"freelancer\">I'm available for freelance</label>\r\n                      </div>\r\n                      <button class=\"btn btn-primary mt-3 mt-sm-0\" type=\"button\" @click=\"updateUser\">Save\r\n                        changes</button>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </section>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport apiClient from '@/services/ApiService';\r\n\r\nexport default {\r\n  components: {\r\n  },\r\n  data() {\r\n    return {\r\n      isCurrentUser: false,\r\n      posts: [],\r\n\r\n      userData: {\r\n        name: '',\r\n        username: '',\r\n        email: '',\r\n        birthdate: '',\r\n        description: '',\r\n        user_picture: ''\r\n      },\r\n      profilePhoto: null,\r\n      profileImageFile: null,\r\n\r\n      publications: 0,\r\n      follows: {\r\n        isFollowing: false,\r\n        followers: 0,\r\n        following: 0,\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchUser();\r\n    this.fetchPosts();\r\n  },\r\n  methods: {\r\n    handleFileUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.profileImageFile = file;\r\n        this.profilePhoto = URL.createObjectURL(file);\r\n      }\r\n    },\r\n    async fetchUser() {\r\n      try {\r\n        const token = localStorage.getItem(\"authToken\");\r\n        const username = this.$route.params.username;\r\n        const response = await apiClient.get(`/profile/${username}`, {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`\r\n          }\r\n        });\r\n        console.log(\"User data:\", response.data);\r\n        this.userData = response.data.user;\r\n        this.isCurrentUser = response.data.isCurrentUser;\r\n      } catch (error) {\r\n        console.error(\"Error al obtener el usuario:\", error.response?.data || error);\r\n        alert(\"Error al obtener los datos del usuario.\");\r\n      }\r\n    },\r\n    async updateUser() {\r\n      const token = localStorage.getItem('authToken');\r\n      if (token) {\r\n        try {\r\n          console.log('Updating user data:', this.userData);\r\n          // const response = await apiClient.put(`/profile/${this.userData.username}/edit-account`, this.userData, {\r\n          //   headers: {\r\n          //     Authorization: `Bearer ${token}`\r\n          //   }\r\n          // });\r\n          // console.log('User updated:', response.data);\r\n\r\n          if (this.profileImageFile) {\r\n            const formData = new FormData();\r\n            formData.append('photo', this.profileImageFile);\r\n            await apiClient.post(`/upload/profile`, formData, {\r\n              headers: {\r\n                Authorization: `Bearer ${token}`,\r\n                'Content-Type': 'multipart/form-data',\r\n              }\r\n            });\r\n          }\r\n\r\n          this.$router.push(`/profile/${this.userData.username}/edit - account`).then(() => {\r\n            window.location.reload();\r\n          });\r\n\r\n        } catch (error) {\r\n          console.error('Error updating profile:', error);\r\n        }\r\n      }\r\n    },\r\n    async fetchPosts() {\r\n      try {\r\n        const token = localStorage.getItem(\"authToken\");\r\n        const username = this.$route.params.username;\r\n        const res = await apiClient.get(\r\n          `/profile/${username}/posts?page=${this.page}&limit=${this.postsPerPage}`,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${token}`,\r\n            },\r\n          }\r\n        );\r\n        this.posts = res.data.posts;\r\n      } catch (error) {\r\n        console.error(\"Error fetching posts:\", error);\r\n      }\r\n    },\r\n    getUserImage() {\r\n      return this.userData.profile_picture ||\r\n        \"https://firebasestorage.googleapis.com/v0/b/booksharing-socialnetwork.appspot.com/o/profile%2Fdefault.jpg?alt=media\";\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.file-input {\r\n  position: relative;\r\n  width: 100%;\r\n}\r\n\r\n.file-input input[type=\"file\"] {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  opacity: 0;\r\n  cursor: pointer;\r\n}\r\n\r\n.file-input-preview {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 10px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.preview-item {\r\n  display: flex;\r\n  align-items: center;\r\n  max-width: 100%;\r\n  flex: 1 1 calc(33.33% - 10px);\r\n  min-width: 80px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.preview-image {\r\n  width: 80px;\r\n  height: 80px;\r\n  object-fit: cover;\r\n  border-radius: 5px;\r\n}\r\n\r\n.preview-filename {\r\n  margin-left: 10px;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n  white-space: nowrap;\r\n}\r\n\r\n.placeholder-text {\r\n  display: block;\r\n  color: #adb5bd;\r\n  text-align: center;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .preview-item {\r\n    flex: 1 1 calc(50% - 10px);\r\n  }\r\n}\r\n\r\n@media (max-width: 480px) {\r\n  .preview-item {\r\n    flex: 1 1 100%;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;;AAyLA,OAAOA,SAAQ,MAAO,uBAAuB;AAE7C,eAAe;EACbC,UAAU,EAAE,CACZ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,KAAK;MACpBC,KAAK,EAAE,EAAE;MAETC,QAAQ,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,SAAS,EAAE,EAAE;QACbC,WAAW,EAAE,EAAE;QACfC,YAAY,EAAE;MAChB,CAAC;MACDC,YAAY,EAAE,IAAI;MAClBC,gBAAgB,EAAE,IAAI;MAEtBC,YAAY,EAAE,CAAC;MACfC,OAAO,EAAE;QACPC,WAAW,EAAE,KAAK;QAClBC,SAAS,EAAE,CAAC;QACZC,SAAS,EAAE;MACb;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;IAChB,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAClC,IAAIF,IAAI,EAAE;QACR,IAAI,CAACZ,gBAAe,GAAIY,IAAI;QAC5B,IAAI,CAACb,YAAW,GAAIgB,GAAG,CAACC,eAAe,CAACJ,IAAI,CAAC;MAC/C;IACF,CAAC;IACD,MAAML,SAASA,CAAA,EAAG;MAChB,IAAI;QACF,MAAMU,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;QAC/C,MAAMzB,QAAO,GAAI,IAAI,CAAC0B,MAAM,CAACC,MAAM,CAAC3B,QAAQ;QAC5C,MAAM4B,QAAO,GAAI,MAAMnC,SAAS,CAACoC,GAAG,CAAC,YAAY7B,QAAQ,EAAE,EAAE;UAC3D8B,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUR,KAAK;UAChC;QACF,
{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport apiClient from '../services/ApiService.js';\nexport default {\n  data() {\n    return {\n      email: '',\n      username: '',\n      password: '',\n      terminos: ''\n    };\n  },\n  methods: {\n    async registerUser() {\n      try {\n        const response = await apiClient.post('/register', {\n          email: this.email,\n          username: this.username,\n          password: this.password\n        });\n        console.log('Registered:', response.data);\n        // Manejar el token o redirigir al usuario\n        localStorage.setItem('authToken', response.data.token);\n        alert(\"USUARIO REGISTRADO CORRECTAMENTE\");\n        this.$router.push('/login'); // Redirige al login después de registrarse\n      } catch (error) {\n        console.error('Error en el registro:', error.response.data);\n      }\n    }\n  }\n};","map":{"version":3,"names":["apiClient","data","email","username","password","terminos","methods","registerUser","response","post","console","log","localStorage","setItem","token","alert","$router","push","error"],"sources":["C:\\DATA\\POW\\BookSharing_SocialNetwork\\fronvue\\src\\components\\register_component.vue"],"sourcesContent":["<template>\r\n  <div class=\"register fadeInDown\">\r\n    <div id=\"formContent\">\r\n      <div class=\"fadeIn first\">\r\n        <img src=\"@/assets/LogoRM.png\" id=\"icon\" alt=\"logo\" />\r\n      </div>\r\n      <form @submit.prevent=\"registerUser\">\r\n        <input type=\"email\" id=\"email\" class=\"fadeIn second\" placeholder=\"Correo Electrónico\" v-model=\"email\" required>\r\n        <input type=\"text\" id=\"username\" class=\"fadeIn third\" placeholder=\"Usuario\" v-model=\"username\" required>\r\n        <input type=\"password\" id=\"password\" class=\"fadeIn fourth\" placeholder=\"Contraseña\" v-model=\"password\" required>\r\n        <div class=\"form-check d-flex justify-content-center\">\r\n          <input type=\"checkbox\" id=\"terminos\" class=\"fadeIn form-check-input\" value=\"\" v-model=\"terminos\" required>\r\n          <label class=\"form-check-label px-3 text-secondary\" for=\"terminos\">Aceptar terminos y condiciones</label>\r\n        </div>\r\n        <input type=\"submit\" class=\"fadeIn fifth\" value=\"Registrate\">\r\n      </form>\r\n      <div id=\"formFooter\">\r\n        <router-link class=\"underlineHover\" to=\"/login\">¿Ya tienes una cuenta? Inicia sesión</router-link>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import apiClient from '../services/ApiService.js';\r\n\r\n  export default {\r\n    data() {\r\n      return {\r\n        email: '',\r\n        username: '',\r\n        password: '',\r\n        terminos:'',\r\n      };\r\n    },\r\n    methods: {\r\n      async registerUser() {\r\n        try {\r\n          const response = await apiClient.post('/register', {\r\n            email: this.email,\r\n            username: this.username,\r\n            password: this.password\r\n          });\r\n          console.log('Registered:', response.data);\r\n          // Manejar el token o redirigir al usuario\r\n          localStorage.setItem('authToken', response.data.token);\r\n          alert(\"USUARIO REGISTRADO CORRECTAMENTE\");\r\n          this.$router.push('/login'); // Redirige al login después de registrarse\r\n        } catch (error) {\r\n          console.error('Error en el registro:', error.response.data);\r\n        }\r\n      }\r\n    }\r\n  };\r\n</script>\r\n\r\n<style scoped>\r\n  html {\r\n    background-color: #56baed;\r\n  }\r\n\r\n  body {\r\n    font-family: \"Poppins\", sans-serif;\r\n    height: 100vh;\r\n  }\r\n\r\n  a {\r\n    color: #92badd;\r\n    display:inline-block;\r\n    text-decoration: none;\r\n    font-weight: 400;\r\n  }\r\n\r\n  h2 {\r\n    text-align: center;\r\n    font-size: 16px;\r\n    font-weight: 600;\r\n    text-transform: uppercase;\r\n    display:inline-block;\r\n    margin: 40px
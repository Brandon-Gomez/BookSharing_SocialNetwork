{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { loginUser, logoutUser } from \"@/services/useAuth.js\";\nimport apiClient from \"@/services/ApiService\";\nexport default {\n  components: {},\n  data() {\n    return {\n      showSearch: false,\n      isAuthenticated: false,\n      isSidebarOpen: false,\n      loginEmail: \"\",\n      loginPassword: \"\",\n      username: \"\",\n      searchQuery: \"\",\n      users: []\n    };\n  },\n  methods: {\n    toggleSidebar() {\n      this.isSidebarOpen = !this.isSidebarOpen;\n    },\n    handleSearch() {\n      this.showSearch = !this.showSearch;\n    },\n    async handleLogin() {\n      try {\n        await loginUser({\n          email: this.loginEmail,\n          password: this.loginPassword,\n          router: this.$router\n        });\n      } catch (error) {\n        alert(\"Credenciales incorrectas\");\n      }\n    },\n    checkAuth() {\n      // Obtener el token del localStorage\n      const token = localStorage.getItem(\"authToken\");\n      this.username = localStorage.getItem(\"username\");\n      if (token) {\n        try {\n          // Validar si el token aún es válido (opcional, puedes decodificarlo)\n          // const decoded = jwt_decode(token);\n          // // Verifica si el token ha expirado (opcionalmente, usando el campo 'exp' del JWT)\n          // const currentTime = Math.floor(Date.now() / 1000);\n          // if (decoded.exp < currentTime) {\n          //   this.isAuthenticated = false;\n          //   localStorage.removeItem('authToken'); // Limpiar token si está expirado\n          //   return;\n          // }\n\n          // Si el token es válido y no ha expirado, el usuario está autenticado\n          this.isAuthenticated = true;\n        } catch (error) {\n          localStorage.removeItem(\"authToken\");\n        }\n      } else {\n        this.$router.push(\"/login\");\n      }\n    },\n    async searchUser() {\n      if (this.searchQuery.length > 2) {\n        try {\n          const response = await apiClient.get(\"/search\", {\n            params: {\n              query: this.searchQuery\n            }\n          });\n          this.users = response.data;\n        } catch (error) {\n          console.error(\"Error al buscar usuarios:\", error);\n        }\n      } else {\n        this.users = [];\n      }\n    },\n    goToProfile(username) {\n      this.$router.push(`/profile/${username}`);\n      this.users = [];\n      this.searchQuery = \"\";\n    },\n    handleClickOutside(event) {\n      const container = this.$refs.searchContainer;\n      const containerMobile = this.$refs.searchContainerMobile;\n      if (container && !container.contains(event.target) && containerMobile && !containerMobile.contains(event.target)) {\n        this.users = [];\n      }\n    },\n    logout() {\n      logoutUser(this.$router);\n    }\n  },\n  mounted() {\n    this.checkAuth();\n    document.addEventListener(\"click\", this.handleClickOutside);\n  },\n  beforeUnmount() {\n    document.removeEventListener(\"click\", this.handleClickOutside);\n  }\n};","map":{"version":3,"names":["loginUser","logoutUser","apiClient","components","data","showSearch","isAuthenticated","isSidebarOpen","loginEmail","loginPassword","username","searchQuery","users","methods","toggleSidebar","handleSearch","handleLogin","email","password","router","$router","error","alert","checkAuth","token","localStorage","getItem","removeItem","push","searchUser","length","response","get","params","query","console","goToProfile","handleClickOutside","event","container","$refs","searchContainer","containerMobile","searchContainerMobile","contains","target","logout","mounted","document","addEventListener","beforeUnmount","removeEventListener"],"sources":["D:\\Downloads\\BookSharing_SocialNetwork\\fronvue\\src\\layouts\\MainLayout.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- Sign in / sign up modal-->\r\n    <div class=\"modal fade\" id=\"signin-modal\" tabindex=\"-1\" role=\"dialog\">\r\n      <div class=\"modal-dialog modal-dialog-centered\" role=\"document\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header bg-secondary\">\r\n            <ul class=\"nav nav-tabs card-header-tabs\" role=\"tablist\">\r\n              <li class=\"nav-item\">\r\n                <a\r\n                  class=\"nav-link fw-medium active\"\r\n                  href=\"#signin-tab\"\r\n                  data-bs-toggle=\"tab\"\r\n                  role=\"tab\"\r\n                  aria-selected=\"true\"\r\n                  ><i class=\"ci-unlocked me-2 mt-n1\"></i>Iniciar sesión</a\r\n                >\r\n              </li>\r\n              <li class=\"nav-item\">\r\n                <a\r\n                  class=\"nav-link fw-medium\"\r\n                  href=\"#signup-tab\"\r\n                  data-bs-toggle=\"tab\"\r\n                  role=\"tab\"\r\n                  aria-selected=\"false\"\r\n                  ><i class=\"ci-user me-2 mt-n1\"></i>Registrarse</a\r\n                >\r\n              </li>\r\n            </ul>\r\n            <button\r\n              class=\"btn-close\"\r\n              type=\"button\"\r\n              data-bs-dismiss=\"modal\"\r\n              aria-label=\"Close\"\r\n            ></button>\r\n          </div>\r\n          <div class=\"modal-body tab-content py-4\">\r\n            <form\r\n              class=\"needs-validation tab-pane fade show active\"\r\n              autocomplete=\"off\"\r\n              novalidate\r\n              id=\"signin-tab\"\r\n              @submit.prevent=\"handleLogin\"\r\n            >\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\" for=\"si-email\">Email</label>\r\n                <input\r\n                  class=\"form-control\"\r\n                  type=\"email\"\r\n                  id=\"si-email\"\r\n                  v-model=\"loginEmail\"\r\n                  placeholder=\"johndoe@example.com\"\r\n                  required\r\n                />\r\n                <div class=\"invalid-feedback\">\r\n                  Indique una dirección de correo electrónico válida.\r\n                </div>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\" for=\"si-password\">Contraseña</label>\r\n                <div class=\"password-toggle\">\r\n                  <input\r\n                    class=\"form-control\"\r\n                    type=\"password\"\r\n                    id=\"si-password\"\r\n                    v-model=\"loginPassword\"\r\n                    required\r\n                  />\r\n                  <label\r\n                    class=\"password-toggle-btn\"\r\n                    aria-label=\"Show/hide password\"\r\n                  >\r\n                    <input class=\"password-toggle-check\" type=\"checkbox\" /><span\r\n                      class=\"password-toggle-indicator\"\r\n                    ></span>\r\n                  </label>\r\n                </div>\r\n              </div>\r\n              <div class=\"mb-3 d-flex flex-wrap justify-content-between\">\r\n                <div class=\"form-check mb-2\">\r\n                  <input\r\n                    class=\"form-check-input\"\r\n                    type=\"checkbox\"\r\n                    id=\"si-remember\"\r\n                  />\r\n                  <label class=\"form-check-label\" for=\"si-remember\"\r\n                    >Recordar contraseña</label\r\n                  >\r\n                </div>\r\n                <a class=\"fs-sm\" href=\"#\">Olvido la contraseña?</a>\r\n              </div>\r\n              <button\r\n                class=\"btn btn-primary btn-shadow d-block w-100\"\r\n                type=\"submit\"\r\n              >\r\n                Iniciar sesión\r\n              </button>\r\n            </form>\r\n            <form\r\n              class=\"needs-validation tab-pane fade\"\r\n              autocomplete=\"off\"\r\n              novalidate\r\n              id=\"signup-tab\"\r\n            >\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\" for=\"su-name\">Full name</label>\r\n                <input\r\n                  class=\"form-control\"\r\n                  type=\"text\"\r\n                  id=\"su-name\"\r\n                  placeholder=\"John Doe\"\r\n                  required\r\n                />\r\n                <div class=\"invalid-feedback\">Please fill in your name.</div>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label for=\"su-email\">Email address</label>\r\n                <input\r\n                  class=\"form-control\"\r\n                  type=\"email\"\r\n                  id=\"su-email\"\r\n                  placeholder=\"johndoe@example.com\"\r\n                  required\r\n                />\r\n                <div class=\"invalid-feedback\">\r\n                  Please provide a valid email address.\r\n                </div>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\" for=\"su-password\">Password</label>\r\n                <div class=\"password-toggle\">\r\n                  <input\r\n                    class=\"form-control\"\r\n                    type=\"password\"\r\n                    id=\"su-password\"\r\n                    required\r\n                  />\r\n                  <label\r\n                    class=\"password-toggle-btn\"\r\n                    aria-label=\"Show/hide password\"\r\n                  >\r\n                    <input class=\"password-toggle-check\" type=\"checkbox\" /><span\r\n                      class=\"password-toggle-indicator\"\r\n                    ></span>\r\n                  </label>\r\n                </div>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\" for=\"su-password-confirm\"\r\n                  >Confirm password</label\r\n                >\r\n                <div class=\"password-toggle\">\r\n                  <input\r\n                    class=\"form-control\"\r\n                    type=\"password\"\r\n                    id=\"su-password-confirm\"\r\n                    required\r\n                  />\r\n                  <label\r\n                    class=\"password-toggle-btn\"\r\n                    aria-label=\"Show/hide password\"\r\n                  >\r\n                    <input class=\"password-toggle-check\" type=\"checkbox\" /><span\r\n                      class=\"password-toggle-indicator\"\r\n                    ></span>\r\n                  </label>\r\n                </div>\r\n              </div>\r\n              <button\r\n                class=\"btn btn-primary btn-shadow d-block w-100\"\r\n                type=\"submit\"\r\n              >\r\n                Sign up\r\n              </button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <main class=\"page-wrapper\">\r\n      <div\r\n        class=\"offcanvas offcanvas-end me-auto order-lg-2\"\r\n        id=\"navbarOffcanvas\"\r\n      >\r\n        <div class=\"offcanvas-header\">\r\n          <h5 class=\"offcanvas-title\">Menu</h5>\r\n          <button\r\n            class=\"btn-close text-reset\"\r\n            type=\"button\"\r\n            data-bs-dismiss=\"offcanvas\"\r\n            aria-label=\"Close\"\r\n          ></button>\r\n        </div>\r\n        <div class=\"offcanvas-body\">\r\n          <ul class=\"navbar-nav w-100 justify-content-around\">\r\n            <li class=\"nav-item\">\r\n              <a class=\"nav-link\" href=\"/\">Inicio</a>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <a class=\"nav-link\" href=\"/category/15\">\r\n                \r\n                <i class=\"ci ci-menu mr-1 \"></i>\r\n                Categorias</a>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <a class=\"nav-link\" :href=\"`/profile/${username}/likes`\">\r\n                 <i class=\"ci ci-heart mr-1 \"></i>\r\n                Favoritos</a>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n            \r\n              <a class=\"nav-link\" :href=\"`/profile/${username}`\">\r\n                  <i class=\"ci ci-user mr-1 \"></i>\r\n                Mi Perfil</a>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <a type=\"button\" class=\"nav-link\" @click=\"logout()\"\r\n                >Cerrar sesión</a\r\n              >\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Navbar 3 Level (Light)-->\r\n      <header class=\"shadow-sm\">\r\n        <!-- Remove \"navbar-sticky\" class to make navigation bar scrollable with the page.-->\r\n        <div class=\"navbar-sticky bg-light\">\r\n          <div class=\"navbar navbar-expand-lg navbar-light py-0\">\r\n            <div class=\"container\">\r\n              <a\r\n                class=\"navbar-brand d-none d-sm-block flex-shrink-0 py-0\"\r\n                href=\"/\"\r\n                ><img\r\n                  src=\"../../dist/img/LogoRM.b5d6692e.png\"\r\n                  width=\"142\"\r\n                  alt=\"Readmate\" /></a\r\n              ><a class=\"navbar-brand d-sm-none flex-shrink-0 me-2\" href=\"/\"\r\n                ><img\r\n                  src=\"../../dist/img/LogoRM.b5d6692e.png\"\r\n                  width=\"74\"\r\n                  alt=\"Readmate\"\r\n              /></a>\r\n              <div class=\"position-relative d-none d-lg-flex mx-4 w-100\" >\r\n                <input\r\n                  ref=\"searchContainer\"\r\n                  v-model=\"searchQuery\"\r\n                  @input=\"searchUser\"\r\n                  class=\"form-control rounded-end pe-5\"\r\n                  type=\"text\"\r\n                  placeholder=\"Buscar un usuario..\"\r\n                />\r\n                <i class=\"ci-search position-absolute top-50 end-0 translate-middle-y text-muted fs-base me-3\"></i>\r\n                <ul\r\n                  v-if=\"users.length\"\r\n                  class=\"list-group position-absolute\"\r\n                  style=\"top: 100%; left: 0; width: 100%; z-index: 1000;\"\r\n                >\r\n                  <li\r\n                    v-for=\"user in users\"\r\n                    :key=\"user.id\"\r\n                    class=\"list-group-item list-group-item-action\"\r\n                    @click=\"goToProfile(user.username)\"\r\n                    style=\"cursor:pointer\"\r\n                  >\r\n                    {{ user.username }} ({{ user.name }})\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n              <div\r\n                class=\"navbar-toolbar d-flex flex-shrink-0 align-items-center\"\r\n              >\r\n                <a class=\"navbar-tool d-none d-lg-flex\" :href=\"`/profile/${username}`\"\r\n                  ><span class=\"navbar-tool-tooltip\">Favoritos</span>\r\n                  <div class=\"navbar-tool-icon-box\">\r\n                    <i class=\"navbar-tool-icon ci-heart\"></i>\r\n                  </div>\r\n                </a>\r\n                <!-- Solo muestra el botón de login si NO está autenticado -->\r\n                <a\r\n                  v-if=\"!isAuthenticated\"\r\n                  class=\"navbar-tool ms-1 ms-lg-0 me-n1 me-lg-2\"\r\n                  href=\"#signin-modal\"\r\n                  data-bs-toggle=\"modal\"\r\n                >\r\n                  <div class=\"navbar-tool-icon-box\">\r\n                    <i class=\"navbar-tool-icon ci-user\"></i>\r\n                  </div>\r\n                  <div class=\"navbar-tool-text ms-n3\">\r\n                    <small>Iniciar Sesión</small>Mi cuenta\r\n                  </div>\r\n                </a>\r\n\r\n                <a\r\n                  :href=\"`/profile/${username}`\"\r\n                  v-else\r\n                  class=\"navbar-tool ms-1 ms-lg-0 me-n1 me-lg-2\"\r\n                >\r\n                  <div class=\"navbar-tool-icon-box\">\r\n                    <i class=\"navbar-tool-icon ci-user\"></i>\r\n                  </div>\r\n                  <div class=\"navbar-tool-text ms-n3\">\r\n                    <small>Mi Perfil</small>\r\n                    <span v-if=\"username\">{{ username }}</span>\r\n                  </div>\r\n                </a>\r\n\r\n                <a\r\n                  class=\"navbar-tool ms-1 ms-lg-0 me-n1 me-lg-2 text-dark d-none d-lg-block\"\r\n                  href=\"#navbarOffcanvas\"\r\n                  data-bs-toggle=\"offcanvas\"\r\n                >\r\n                  <div class=\"navbar-tool-icon-box\">\r\n                    <i class=\"navbar-tool-icon ci-menu\"></i>\r\n                  </div>\r\n                </a>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!-- Menu movil -->\r\n          <div\r\n            class=\"navbar navbar-expand-lg navbar-light navbar-stuck-menu mt-n2 pt-0 pb-2 d-lg-none\"\r\n          >\r\n            <div class=\"container\">\r\n              <div class=\"collapse navbar-collapse\" id=\"navbarCollapse\">\r\n                <div class=\"input-group d-lg-none mt-3\" ref=\"searchContainerMobile\">\r\n                  <i\r\n                    class=\"ci-search position-absolute top-50 start-0 translate-middle-y text-muted fs-base ms-3\"\r\n                  ></i>\r\n                  <input\r\n                    class=\"form-control rounded-start\"\r\n                    type=\"text\"\r\n                    placeholder=\"Buscar un usuario..\"\r\n                    v-model=\"searchQuery\"\r\n                    @input=\"searchUser\"\r\n                  />\r\n                </div>\r\n                <ul\r\n                  v-if=\"users.length\"\r\n                  class=\"list-group  w-100\"\r\n                  style=\"z-index: 1000\"\r\n                >\r\n                  <li\r\n                    v-for=\"user in users\"\r\n                    :key=\"user.id\"\r\n                    class=\"list-group-item list-group-item-action\"\r\n                    @click=\"goToProfile(user.username)\"\r\n                  >\r\n                    {{ user.username }} ({{ user.name }})\r\n                  </li>\r\n                </ul>\r\n                <ul class=\"navbar-nav w-100 justify-content-around mt-3\">\r\n                  <li class=\"nav-item\">\r\n                    <a class=\"nav-link\" href=\"/\">Inicio</a>\r\n                  </li>\r\n                  <li class=\"nav-item\">\r\n                    <a class=\"nav-link\" href=\"/category/15\">Categorías</a>\r\n                  </li>\r\n                  <li class=\"nav-item\">\r\n                    <a class=\"nav-link\" href=\"/likes\">Favoritos</a>\r\n                  </li>\r\n                  <li class=\"nav-item\">\r\n                    <a class=\"nav-link\" :href=\"`/profile/${username}`\"\r\n                      >Mi Perfil</a\r\n                    >\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </header>\r\n      <slot />\r\n    </main>\r\n    <!-- Footer-->\r\n    <footer class=\"footer py-5 bg-dark\">\r\n      <div class=\"container pt-2 pb-1\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-6 text-center text-md-start mb-2\">\r\n            <div class=\"text-nowrap mb-3\">\r\n              <a class=\"d-inline-block align-middle mt-n2 me-2\" href=\"#\"\r\n                ><img\r\n                  class=\"d-block\"\r\n                  src=\"../../dist/img/logo2.png\"\r\n                  width=\"117\"\r\n                  alt=\"Readmate\"\r\n              /></a>\r\n            </div>\r\n            <p class=\"fs-sm text-white opacity-70 pb-1\">\r\n              Lee, comparte, construye comunidad.\r\n            </p>\r\n            <h6 class=\"d-inline-block pe-3 me-3 border-end border-light\">\r\n              <span class=\"text-primary\">589 </span\r\n              ><span class=\"fw-normal text-white\">Lectores</span>\r\n            </h6>\r\n            <h6 class=\"d-inline-block pe-3 me-3 border-light\">\r\n              <span class=\"text-primary\">336 </span\r\n              ><span class=\"fw-normal text-white\">Libros</span>\r\n            </h6>\r\n            <div class=\"widget mt-4 text-md-nowrap text-center text-md-start\">\r\n              <a class=\"btn-social bs-light bs-twitter me-2 mb-2\" href=\"#\"\r\n                ><i class=\"ci-instagram\"></i></a\r\n              ><a class=\"btn-social bs-light bs-facebook me-2 mb-2\" href=\"#\"\r\n                ><i class=\"ci-facebook\"></i></a\r\n              ><a class=\"btn-social bs-light bs-dribbble me-2 mb-2\" href=\"#\"\r\n                ><i class=\"ci-tiktok\"></i\r\n              ></a>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Desktop menu (visible on screens above md)-->\r\n          <div class=\"col-md-3 d-none d-md-block text-center text-md-start\">\r\n            <div class=\"widget widget-links widget-light pb-2\">\r\n              <h3 class=\"widget-title text-light\">Categorias</h3>\r\n              <ul class=\"widget-list\">\r\n                <li class=\"widget-list-item\">\r\n                  <router-link class=\"widget-list-link\" :to=\"'/category/16'\"\r\n                    >Ficción</router-link\r\n                  >\r\n                </li>\r\n                <li class=\"widget-list-item\">\r\n                  <router-link class=\"widget-list-link\" :to=\"'/category/17'\"\r\n                    >No ficción</router-link\r\n                  >\r\n                </li>\r\n                <li class=\"widget-list-item\">\r\n                  <router-link class=\"widget-list-link\" :to=\"'/category/18'\"\r\n       
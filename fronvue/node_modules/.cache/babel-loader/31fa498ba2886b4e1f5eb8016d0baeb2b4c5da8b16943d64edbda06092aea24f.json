ario\r\n        } catch (error) {\r\n          console.error('Error en el registro:', error.response.data);\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  /* Estilos para tu formulario de registro */\r\n  </style>\r\n  "],"mappings":"AAYE,OAAOA,SAAQ,MAAO,2BAA2B;AAEjD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,SAAS,CAACQ,IAAI,CAAC,WAAW,EAAE;UACjDN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACFK,OAAO,CAACC,GAAG,CAAC,aAAa,EAAEH,QAAQ,CAACN,IAAI,CAAC;QACzC;MACF,EAAE,OAAOU,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAACJ,QAAQ,CAACN,IAAI,CAAC;MAC7D;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
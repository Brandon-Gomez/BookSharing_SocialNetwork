{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport apiClient from \"@/services/ApiService\";\nexport default {\n  props: {\n    postId: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      newPost: {\n        title: \"\",\n        description: \"\",\n        images: null,\n        pdf_file: null\n      },\n      userData: {},\n      isCurrentUser: false,\n      follows: {\n        followers: 0,\n        following: 0\n      },\n      posts: [],\n      profileData: {}\n    };\n  },\n  mounted() {\n    this.profileData = JSON.parse(localStorage.getItem(\"profileData\")) || {};\n    this.fetchUser();\n    this.fetchPost();\n  },\n  methods: {\n    getUserImage() {\n      return this.userData.profile_picture || \"https://firebasestorage.googleapis.com/v0/b/booksharing-socialnetwork.appspot.com/o/profile%2Fdefault.jpg?alt=media\";\n    },\n    async fetchUser() {\n      try {\n        const token = localStorage.getItem(\"authToken\");\n        const username = this.$route.params.username;\n        const response = await apiClient.get(`/profile/${username}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        let user = response.data.user;\n        if (user.birthdate) {\n          user.birthdate = user.birthdate.split(\"T\")[0];\n        }\n        this.userData = {\n          ...user,\n          password: \"\"\n        };\n        this.isCurrentUser = response.data.isCurrentUser;\n\n        // Seguidores y seguidos\n        if (user.id) {\n          const resFollowers = await apiClient.get(`/follow/followers/count/${user.id}`, {\n            headers: {\n              Authorization: `Bearer ${token}`\n            }\n          });\n          this.follows.followers = resFollowers.data.followerCount || 0;\n          const resFollowing = await apiClient.get(`/follow/following/count/${user.id}`, {\n            headers: {\n              Authorization: `Bearer ${token}`\n            }\n          });\n          this.follows.following = resFollowing.data.followingCount || 0;\n        }\n      } catch (error) {\n        console.error(\"Error al obtener el usuario:\", error.response?.data || error);\n        alert(\"Error al obtener los datos del usuario.\");\n      }\n    },\n    async fetchPost() {\n      try {\n        const response = await apiClient.get(`/posts/${this.postId}`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem(\"authToken\")}`\n          }\n        });\n        this.newPost = {\n          ...response.data,\n          images: null,\n          // Para evitar sobreescribir si no se cambia\n          pdf_file: null\n        };\n      } catch (error) {\n        console.error(\"Error fetching post:\", error);\n        alert(\"No se pudo cargar la publicación.\");\n      }\n    },\n    handleImageUpload(event) {\n      this.newPost.images = event.target.files[0];\n    },\n    handlePdfUpload(event) {\n      this.newPost.pdf_file = event.target.files[0];\n    },\n    async updatePost() {\n      const token = localStorage.getItem(\"authToken\");\n      if (!this.newPost.title || !this.newPost.description) {\n        alert(\"El título y la descripción son obligatorios.\");\n        return;\n      }\n      if (token) {\n        const formData = new FormData();\n        formData.append(\"title\", this.newPost.title);\n        formData.append(\"description\", this.newPost.description);\n        if (this.newPost.images) {\n          formData.append(\"images\", this.newPost.images);\n        }\n        if (this.newPost.pdf_file) {\n          formData.append(\"pdf\", this.newPost.pdf_file);\n        }\n        try {\n          await apiClient.put(`/posts/${this.postId}`, formData, {\n            headers: {\n              Authorization: `Bearer ${token}`,\n              \"Content-Type\": \"multipart/form-data\"\n            }\n          });\n          alert(\"Publicación actualizada correctamente.\");\n          this.$router.push(`/profile/${this.userData.username}`);\n        } catch (error) {\n          console.error(\"Error updating post:\", error);\n          alert(\"Error al actualizar la publicación.\");\n        }\n      }\n    },\n    async deletePost() {\n      if (!confirm(\"¿Estás seguro de que deseas eliminar esta publicación?\")) return;\n      try {\n        const token = localStorage.getItem(\"authToken\");\n        await apiClient.delete(`/posts/${this.postId}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        alert(\"Publicación eliminada correctamente.\");\n        this.$router.push(`/profile/${this.userData.username}`);\n      } catch (error) {\n        console.error(\"Error deleting post:\", error);\n        alert(\"Error al eliminar la publicación.\");\n      }\n    },\n    goToEditProfile() {\n      this.$router.push(`/profile/${this.userData.username}/edit-account`);\n    },\n    logout() {\n      localStorage.removeItem(\"authToken\");\n      this.$router.push(\"/login\");\n    }\n    // Si necesitas cargar publicaciones del usuario para la barra lateral\n    // async fetchPosts() {\n    //   // ...\n    // },\n  }\n};","map":{"version":3,"names":["apiClient","props","postId","type","String","required","data","newPost","title","description","images","pdf_file","userData","isCurrentUser","follows","followers","following","posts","profileData","mounted","JSON","parse","localStorage","getItem","fetchUser","fetchPost","methods","getUserImage","profile_picture","token","username","$route","params","response","get","headers","Authorization","user","birthdate","split","password","id","resFollowers","followerCount","resFollowing","followingCount","error","console","alert","handleImageUpload","event","target","files","handlePdfUpload","updatePost","formData","FormData","append","put","$router","push","deletePost","confirm","delete","goToEditProfile","logout","removeItem"],"sources":["D:\\Downloads\\BookSharing_SocialNetwork\\fronvue\\src\\components\\editpost_component.vue"],"sourcesContent":["<template>\r\n  <!-- <div v-if=\"newPost\" class=\"edit-post-container container my-4 py-4\">\r\n      <h2>Editar Publicación</h2>\r\n      <form @submit.prevent=\"updatePost\">\r\n        <div class=\"form-group my-2\">\r\n          <label for=\"title\">Título</label>\r\n          <input type=\"text\" id=\"title\" v-model=\"newPost.title\" class=\"form-control\" />\r\n        </div>\r\n  \r\n        <div class=\"form-group my-2\">\r\n          <label for=\"description\">Descripción</label>\r\n          <textarea id=\"description\" v-model=\"newPost.description\" class=\"form-control\"></textarea>\r\n        </div>\r\n  \r\n        <div class=\"form-group my-2\">\r\n          <label for=\"image\">Imagen</label>\r\n          <input type=\"file\" id=\"image\" @change=\"handleImageUpload\" class=\"form-control\" accept=\"image/*\"/>\r\n        </div>\r\n  \r\n        <div class=\"form-group my-2\">\r\n          <label for=\"pdf\">Archivo PDF</label>\r\n          <input type=\"file\" id=\"pdf\" @change=\"handlePdfUpload\" class=\"form-control\" accept=\"application/*\"/>\r\n        </div>\r\n        <button type=\"submit\" class=\"btn btn-primary\">Guardar Cambios</button>\r\n        <button @click=\"deletePost\" class=\"btn btn-danger ml-2\">Eliminar Publicación</button>\r\n      </form>\r\n    </div> -->\r\n\r\n  <div>\r\n   <div>\r\n    <div class=\"page-title-overlap bg-dark pt-4\">\r\n      <div\r\n        class=\"container d-flex flex-wrap flex-sm-nowrap justify-content-center justify-content-sm-between align-items-center pt-2\"\r\n      >\r\n        <div class=\"d-flex align-items-center pb-3\">\r\n          <div\r\n            class=\"rounded-circle position-relative flex-shrink-0\"\r\n            style=\"width: 6.375rem\"\r\n          >\r\n            <img\r\n              class=\"rounded-circle\"\r\n              :src=\"getUserImage()\"\r\n              :alt=\"userData.name\"\r\n            />\r\n          </div>\r\n          <div class=\"ps-3\">\r\n            <h3 class=\"text-light fs-lg mb-0\">{{ userData.name }}</h3>\r\n            <span class=\"d-block text-light fs-ms opacity-60 py-1\"\r\n              >@{{ userData.username }}</span>\r\n            <span class=\"d-block text-light fs-ms py-1 w-75\" style=\"max-width: 450px\">\r\n              {{ userData.description }}</span\r\n            >\r\n\r\n          </div>\r\n        </div>\r\n        <!-- Desktop version -->\r\n        <div class=\"d-none d-sm-flex\"> \r\n          <div class=\"text-sm-end me-2\">\r\n            <div class=\"text-light fs-base mr-2\">{{ posts.length }}<span class=\"text-light fs-ms opacity-60 py-1 ml-2\">Publicaciones</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-sm-end me-2\">\r\n            <div class=\"text-light fs-base mr-2\">{{ follows.followers }}<span class=\"text-light fs-ms opacity-60 py-1 ml-2\">Seguidores</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-sm-end me-2\">\r\n            <div class=\"text-light fs-base mr-2\">{{ follows.following }}<span class=\"text-light fs-ms opacity-60 py-1  ml-2\">Siguiendo</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- Mobile version -->\r\n        <div class=\"d-flex d-sm-none w-100 justify-content-around my-3\">\r\n          <div class=\"text-center\">\r\n            <div class=\"text-light fs-6 fw-bold\">{{ posts.length }}</div>\r\n            <div class=\"text-light fs-ms opacity-60 py-1\">Publicaciones</div>\r\n          </div>\r\n          \r\n          <div class=\"text-center\">\r\n            <div class=\"text-light fs-6 fw-bold\">{{ follows.followers }}</div>\r\n            <div class=\"text-light fs-ms opacity-60 py-1\">Seguidores</div>\r\n          </div>\r\n          <div class=\"text-center\">\r\n            <div class=\"text-light fs-6 fw-bold\">{{ follows.following }}</div>\r\n            <div class=\"text-light fs-ms opacity-60 py-1\">Seguidos</div>\r\n          </div>\r\n        </div>\r\n\r\n\r\n      </div>\r\n    </div>\r\n    <div class=\"container mb-5 pb-3\">\r\n      <div class=\"bg-light shadow-lg rounded-3 overflow-hidden\">\r\n        <div class=\"row\">\r\n          <!-- Sidebar-->\r\n          <aside class=\"col-lg-4 pt-4 pt-lg-0 pe-xl-5\">\r\n            <div class=\"d-block d-lg-none p-4\">\r\n              <a\r\n                class=\"btn btn-outline-accent d-block\"\r\n                href=\"#account-menu\"\r\n                data-bs-toggle=\"collapse\"\r\n                aria-expanded=\"true\"\r\n                ><i class=\"ci-menu me-2\"></i>Account menu</a\r\n              >\r\n            </div>\r\n            <div class=\"bg-white rounded-3 shadow-lg pt-1 mb-lg-0\">\r\n              <div class=\"d-lg-block collapse\" id=\"account-menu\">\r\n                <div class=\"bg-secondary px-4 py-3\">\r\n                  <h3 class=\"fs-sm mb-0 text-muted\">Cuenta</h3>\r\n                </div>\r\n                <ul class=\"list-unstyled mb-0\">\r\n                  <li class=\"border-bottom mb-0\">\r\n                    <a\r\n                      class=\"nav-link-style d-flex align-items-center px-4 py-3 active\"\r\n                      href=\"#\"\r\n                    >\r\n                      <i class=\"ci-book opacity-60 me-2\"></i>\r\n                      Publicaciones\r\n                    </a>\r\n                  </li>\r\n                  <li class=\"border-bottom mb-0\">\r\n                    <a\r\n                      class=\"nav-link-style d-flex align-items-center px-4 py-3\"\r\n                      href=\"#\"\r\n                    >\r\n                      <i class=\"ci-heart opacity-60 me-2\"></i>\r\n                      Favoritos\r\n                    </a>\r\n                  </li>\r\n                </ul>\r\n                <div v-if=\"isCurrentUser\">\r\n                  <div class=\"bg-secondary px-4 py-3\">\r\n                    <h3 class=\"fs-sm mb-0 text-muted\">Configuración</h3>\r\n                  </div>\r\n                  <ul class=\"list-unstyled mb-0\">\r\n                    <li class=\"border-bottom mb-0\">\r\n                      <a\r\n                        type=\"button\"\r\n                        class=\"nav-link-style d-flex align-items-center px-4 py-3\"\r\n                        @click.prevent=\"goToEditProfile\"\r\n                      >\r\n                        <i class=\"ci-user opacity-60 me-2\"></i>\r\n                        Información personal\r\n                      </a>\r\n                    </li>\r\n                    <li class=\"border-top mb-0\">\r\n                      <a type=\"button\"\r\n                        class=\"nav-link-style d-flex align-items-center px-4 py-3\"\r\n                       \r\n                        @click=\"logout\"\r\n                      >\r\n                        <i class=\"ci-sign-out opacity-60 me-2\"></i>\r\n                        Cerrar sesión\r\n                      </a>\r\n                    </li>\r\n                  </ul>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </aside>\r\n          <!-- Content-->\r\n          <section class=\"col-lg-8 pt-lg-4 pb-4 mb-3\">\r\n            <!-- Toolbar-->\r\n\r\n            <h2\r\n              class=\"h3 pt-2 pb-4 mb-0 text-center text-sm-start border-bottom mt-2\"\r\n            >\r\n              Publicaciones\r\n            </h2>\r\n            <!-- grid books -->\r\n            <div class=\"container mb-2 mb-md-4 library-grid py-3\">\r\n              <article\r\n                class=\"library-item\"\r\n                v-for=\"post in posts\"\r\n                :key=\"post.id\"\r\n              >\r\n                <div class=\"card\">\r\n                  <button\r\n                    class=\"btn-wishlist btn-sm position-absolute\"\r\n                    type=\"button\"\r\n                    data-bs-toggle=\"tooltip\"\r\n                    data-bs-placement=\"left\"\r\n                    title=\"Me gusta\"\r\n                    style=\"\r\n                      top: 0.5rem;\r\n                      right: 0.5rem;\r\n                      z-index: 2;\r\n                      background: rgba(255, 255, 255, 0.7);\r\n                      border-radius: 50%;\r\n                    \"\r\n                  >\r\n                    <i class=\"ci-heart\"></i>\r\n                  </button>\r\n                  <div style=\"position: relative;\">\r\n                    <RouterLink\r\n                      class=\"blog-entry-thumb\"\r\n                      :to=\"`/posts/${post.id}`\"\r\n                    >\r\n                      <img\r\n                        class=\"card-img-top\"\r\n                        :src=\"post.image\"\r\n                        alt=\"Post\"\r\n                      />\r\n                    </RouterLink>\r\n                    <div\r\n                      v-if=\"isCurrentUser\"\r\n                      class=\"position-absolute\"\r\n                      style=\"right: 10px; bottom: 10px; display: flex; gap: 8px;\"\r\n                    >\r\n                      <button\r\n                        class=\"btn btn-warning btn-sm d-flex align-items-center\"\r\n                        @click=\"editPost(post)\"\r\n                        title=\"Editar\"\r\n                      >\r\n                        <i class=\"ci-edit\"></i>\r\n                      </button>\r\n                      <button\r\n                        class=\"btn btn-danger btn-sm d-flex align-items-center\"\r\n                        @click=\"deletePost(post)\"\r\n                        title=\"Eliminar\"\r\n                      >\r\n                        <i class=\"ci-trash\"></i>\r\n                      </button>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"card-body\">\r\n                    <h2 class=\"h6 blog-entry-title\">\r\n                      <RouterLink :to=\"`/posts/${post.id}`\">\r\n                        {{ post.title }}\r\n                      </RouterLink>\r\n                    </h2>\r\n                    <a\r\n                      class=\"btn-tag me-2 mb-2\"\r\n                      :href=\"`/category/${post.category_id}`\"\r\n                      >{{ post.category_name }}</a\r\n                    >\r\n                  </div>\r\n                  <div class=\"card-footer d-flex align-items-center fs-xs\">\r\n                    {{\r\n                      new Date(post.created_at).toLocaleString(\"es-ES\", {\r\n                        month: \"short\",\r\n                        day: \"2-digit\",\r\n                        year: \"numeric\",\r\n                      })\r\n                    }}\r\n\r\n                    <div class=\"ms-auto text-nowrap\">\r\n                      <a class=\"blog-entry-meta-link text-nowrap\">\r\n                        <i class=\"ci-eye\"></i>{{ post.views }}\r\n                      </a>\r\n                      <span class=\"blog-entry-meta-divider mx-2\"></span>\r\n                      <a class=\"blog-entry-meta-link text-nowrap\">\r\n                        <i class=\"ci-heart\"></i>{{ post.total_likes }}\r\n                      </a>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </article>\r\n            </div>\r\n            <!-- Pagination-->\r\n            <pagination_component\r\n              v-model=\"page\"\r\n              :totalPages=\"totalPages\"\r\n              @update:modelValue=\"onPageChange\"\r\n            />\r\n          </section>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport apiClient from \"@/services/ApiService\";\r\n\r\nexport default {\r\n  props: {\r\n    postId: {\r\n      type: String,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      newPost: {\r\n        title: \"\",\r\n        description: \"\",\r\n        images: null,\r\n        pdf_file: null,\r\n      },\r\n      userData: {},\r\n      isCurrentUser: false,\r\n      follows: {\r\n        followers: 0,\r\n        following: 0,\r\n      },\r\n      posts: [],\r\n      profileData: {},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.profileData = JSON.parse(localStorage.getItem(\"profileData\")) || {};\r\n    this.fetchUser();\r\n    this.fetchPost();\r\n  },\r\n  methods: {\r\n    getUserImage() {\r\n      return (\r\n        this.userData.profile_picture ||\r\n        \"https://firebasestorage.googleapis.com/v0/b/booksharing-socialnetwork.appspot.com/o/profile%2Fdefault.jpg?alt=media\"\r\n      );\r\n    },\r\n    async fetchUser() {\r\n      try {\r\n        const token = localStorage.getItem(\"authToken\");\r\n        const username = this.$route.params.username;\r\n        const response = await apiClient.get(`/profile/${username}`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        let user = response.data.user;\r\n        if (user.birthdate) {\r\n          user.birthdate = user.birthdate.split(\"T\")[0];\r\n        }\r\n        this.userData = { ...user, password: \"\" };\r\n        this.isCurrentUser = response.data.isCurrentUser;\r\n\r\n        // Seguidores y seguidos\r\n        if (user.id) {\r\n          const resFollowers = await apiClient.get(\r\n            `/follow/followers/count/${user.id}`,\r\n            { headers: { Authorization: `Bearer ${token}` } }\r\n          );\r\n          this.follows.followers = resFollowers.data.followerCount || 0;\r\n\r\n          const resFollowing = await apiClient.get(\r\n            `/follow/following/count/${user.id}`,\r\n            { headers: { Authorization: `Bearer ${token}` } }\r\n          );\r\n          this.follows.following = resFollowing.data.followingCount || 0;\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error al obtener el usuario:\", error.response?.data || error);\r\n        alert(\"Error al obtener los datos del usuario.\");\r\n      }\r\n    },\r\n    async fetchPost() {\r\n      try {\r\n        const response = await apiClient.get(`/posts/${this.postId}`,\r\n          {\r\n            headers: {\r\n              Authorization: `Bearer ${localStorage.getItem(\"authToken\")}`,\r\n            },\r\n          }\r\n        );\r\n        this.newPost = {\r\n          ...response.data,\r\n          images: null, // Para evitar sobreescribir si no se cambia\r\n          pdf_file: null,\r\n        };\r\n      } catch (error) {\r\n        console.error(\"Error fetching post:\", error);\r\n        alert(\"No se pudo cargar la publicación.\");\r\n      }\r\n    },\r\n    handleImageUpload(event) {\r\n      this.newPost.images = event.target.files[0];\r\n    },\r\n    handlePdfUpload(event) {\r\n      this.newPost.pdf_file = event.target.files[0];\r\n    },\r\n    async updatePost() {\r\n      const token = localStorage.getItem(\"authToken\");\r\n      if (!this.newPost.title || !this.newPost.description) {\r\n        alert(\"El título y la descripción son obligatorios.\");\r\n        return;\r\n      }\r\n      if (token) {\r\n        const formData = new FormData();\r\n        formData.append(\"title\", this.newPost.title);\r\n        formData.append(\"description\", this.newPost.description);\r\n\r\n        if (this.newPost.images) {\r\n          formData.append(\"images\", this.newPost.images);\r\n        }\r\n        if (this.newPost.pdf_file) {\r\n          formData.append(\"pdf\", this.newPost.pdf_file);\r\n        }\r\n\r\n        try {\r\n          await apiClient.put(\r\n            `/posts/${this.postId}`,\r\n            formData,\r\n            {\r\n              headers: {\r\n                Authorization: `Bearer ${token}`,\r\n                \"Content-Type\": \"multipart/form-data\",\r\n              },\r\n            }\r\n          );\r\n          alert(\"Publicación actualizada correctamente.\");\r\n          this.$router.push(`/profile/${this.userData.username}`);\r\n        } catch (error) {\r\n          console.error(\"Error updating post:\", error);\r\n          alert(\"Error al actualizar la publicación.\");\r\n        }\r\n      }\r\n    },\r\n    async deletePost() {\r\n      if (!confirm(\"¿Estás seguro de que deseas eliminar esta publicación?\")) return;\r\n      try {\r\n        const token = localStorage.getItem(\"authToken\");\r\n        await apiClient.delete(`/posts/${this.postId}`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        alert(\"Publicación eliminada correctamente.\");\r\n        this.$router.push(`/profile/${this.userData.username}`);\r\n      } catch (error) {\r\n        console.error(\"Error deleting post:\", error);\r\n        alert(\"Error al eliminar la publicación.\");\r\n      }\r\n    },\r\n    goToEditProfile() {\r\n      this.$router.push(`/profile/${this.userData.username}/edit-account`);\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"authToken\");\r\n      this.$router.push(\"/login\");\r\n    },\r\n    // Si necesitas cargar publicaciones del usuario para la barra lateral\r\n    // async fetchPosts() {\r\n    //   // ...\r\n    // },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.edit-post-container {\r\n  background-color: rgba(255, 255, 255, 0.9);\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n</style>\r\n"],"mappings":";AAiRA,OAAOA,SAAQ,MAAO,uBAAuB;AAE7C,eAAe;EACbC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,MAAM,EAAE,IAAI;QACZC,QAAQ,EAAE;MACZ,CAAC;MACDC,QAAQ,EAAE,CAAC,CAAC;MACZC,aAAa,EAAE,KAAK;MACpBC,OAAO,EAAE;QACPC,SAAS,EAAE,CAAC;QACZC,SAAS,EAAE;MACb,CAAC;MACDC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,CAAC;IACh
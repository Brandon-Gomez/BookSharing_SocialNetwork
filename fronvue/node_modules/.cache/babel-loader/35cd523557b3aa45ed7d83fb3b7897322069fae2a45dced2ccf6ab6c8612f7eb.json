{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport apiClient from \"@/services/ApiService\";\nexport default {\n  data() {\n    return {\n      userData: {\n        name: \"\",\n        username: \"\",\n        description: \"\",\n        profile_picture: \"\"\n      },\n      countPosts: 0,\n      follows: {\n        followers: 0,\n        following: 0\n      },\n      isCurrentUser: false\n    };\n  },\n  methods: {\n    getUserImage() {\n      return this.userData.profile_picture || \"https://firebasestorage.googleapis.com/v0/b/booksharing-socialnetwork.appspot.com/o/profile%2Fdefault.jpg?alt=media\";\n    },\n    goToEditProfile() {\n      this.$router.push(`/profile/${this.userData.username}/edit-account`);\n    },\n    logout() {\n      localStorage.removeItem(\"authToken\");\n      localStorage.removeItem(\"profileData\");\n      this.$router.push(\"/login\");\n    },\n    async fetchUserData() {\n      try {\n        const token = localStorage.getItem(\"authToken\");\n        if (!token) return;\n\n        // Puedes usar userId como prop o extraerlo de la ruta\n        const id = this.userId || this.$route.params.id;\n        const username = this.$route.params.username;\n\n        // Obtener datos de usuario\n        const resUser = await apiClient.get(`/user/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.userData = resUser.data;\n\n        // Obtener contadores\n        const resPosts = await apiClient.get(`/user/${id}/posts/count`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.countPosts = resPosts.data.count;\n        const resFollows = await apiClient.get(`/user/${id}/follows`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.follows = resFollows.data;\n\n        // Verificar si es el usuario actual\n        const profileData = JSON.parse(localStorage.getItem(\"profileData\"));\n        this.isCurrentUser = profileData && profileData._id === id;\n      } catch (error) {\n        console.error(\"Error al cargar datos de usuario:\", error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchUserData();\n  }\n};","map":{"version":3,"names":["apiClient","data","userData","name","username","description","profile_picture","countPosts","follows","followers","following","isCurrentUser","methods","getUserImage","goToEditProfile","$router","push","logout","localStorage","removeItem","fetchUserData","token","getItem","id","userId","$route","params","resUser","get","headers","Authorization","resPosts","count","resFollows","profileData","JSON","parse","_id","error","console","mounted"],"sources":["D:\\Downloads\\BookSharing_SocialNetwork\\fronvue\\src\\layouts\\ProfileLayout.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- Page Title -->\r\n    <div class=\"page-title-overlap bg-dark pt-4\">\r\n      <div\r\n        class=\"container d-flex flex-wrap flex-sm-nowrap justify-content-center justify-content-sm-between align-items-center pt-2\"\r\n      >\r\n        <div class=\"d-flex align-items-center pb-3\">\r\n          <div\r\n            class=\"rounded-circle position-relative flex-shrink-0\"\r\n            style=\"width: 6.375rem\"\r\n          >\r\n            <img\r\n              class=\"rounded-circle\"\r\n              :src=\"getUserImage()\"\r\n              alt=\"Imagen de perfil\"\r\n            />\r\n          </div>\r\n          <div class=\"ps-3\">\r\n            <h3 class=\"text-light fs-lg mb-0\">{{ userData.name }}</h3>\r\n            <span class=\"d-block text-light fs-ms opacity-60 py-1\"\r\n              >@{{ userData.username }}</span\r\n            >\r\n            <span class=\"d-block text-light fs-ms py-1 w-75\" style=\"max-width: 450px\">\r\n              {{ userData.description }}\r\n            </span>\r\n          </div>\r\n        </div>\r\n        <!-- Desktop version -->\r\n        <div class=\"d-none d-sm-flex\">\r\n          <div class=\"text-sm-end me-2\">\r\n            <div class=\"text-light fs-base mr-2\">{{ countPosts }}<span\r\n                class=\"text-light fs-ms opacity-60 py-1 ml-2\">Publicaciones</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-sm-end me-2\">\r\n            <div class=\"text-light fs-base mr-2\">{{ follows.followers }}<span\r\n                class=\"text-light fs-ms opacity-60 py-1 ml-2\">Seguidores</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-sm-end me-2\">\r\n            <div class=\"text-light fs-base mr-2\">{{ follows.following }}<span\r\n                class=\"text-light fs-ms opacity-60 py-1  ml-2\">Siguiendo</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- Mobile version -->\r\n        <div class=\"d-flex d-sm-none w-100 justify-content-around my-3\">\r\n          <div class=\"text-center\">\r\n            <div class=\"text-light fs-6 fw-bold\">{{ countPosts }}</div>\r\n            <div class=\"text-muted fs-xs\">Publicaciones</div>\r\n          </div>\r\n          <div class=\"text-center\">\r\n            <div class=\"text-light fs-6 fw-bold\">{{ follows.followers }}</div>\r\n            <div class=\"text-muted fs-xs\">Seguidores</div>\r\n          </div>\r\n          <div class=\"text-center\">\r\n            <div class=\"text-light fs-6 fw-bold\">{{ follows.following }}</div>\r\n            <div class=\"text-muted fs-xs\">Seguidos</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"container mb-5 pb-3\">\r\n      <div class=\"bg-light shadow-lg rounded-3 overflow-hidden\">\r\n        <div class=\"row\">\r\n          <!-- Sidebar-->\r\n          <aside class=\"col-lg-4 pt-4 pt-lg-0 pe-xl-5\">\r\n            <div class=\"d-block d-lg-none p-4\">\r\n              <a\r\n                class=\"btn btn-outline-accent d-block\"\r\n                href=\"#account-menu\"\r\n                data-bs-toggle=\"collapse\"\r\n                aria-expanded=\"true\"\r\n                ><i class=\"ci-menu me-2\"></i>Menu de cuenta</a\r\n              >\r\n            </div>\r\n            <div class=\"bg-white rounded-3 shadow-lg pt-1 mb-lg-0\">\r\n              <div class=\"d-lg-block collapse\" id=\"account-menu\">\r\n                <div class=\"bg-secondary px-4 py-3\">\r\n                  <h3 class=\"fs-sm mb-0 text-muted\">Cuenta</h3>\r\n                </div>\r\n                <ul class=\"list-unstyled mb-0\">\r\n                  <li class=\"border-bottom mb-0\">\r\n                    <a class=\"nav-link-style d-flex align-items-center px-4 py-3\" :href=\"`/profile/${userData.username}`\">\r\n                      <i class=\"ci-book opacity-60 me-2\"></i>\r\n                      Publicaciones\r\n                    </a>\r\n                  </li>\r\n                  <li class=\"border-bottom mb-0\">\r\n                    <a class=\"nav-link-style d-flex align-items-center px-4 py-3\" :href=\"`/profile/${userData.username}/likes`\">\r\n                      <i class=\"ci-heart opacity-60 me-2\"></i>\r\n                      Favoritos\r\n                    </a>\r\n                  </li>\r\n                </ul>\r\n                <div v-if=\"isCurrentUser\">\r\n                  <div class=\"bg-secondary px-4 py-3\">\r\n                    <h3 class=\"fs-sm mb-0 text-muted\">Configuración</h3>\r\n                  </div>\r\n                  <ul class=\"list-unstyled mb-0\">\r\n                    <li class=\"border-bottom mb-0\">\r\n                      <a type=\"button\" class=\"nav-link-style d-flex align-items-center px-4 py-3\"\r\n                        @click.prevent=\"goToEditProfile\">\r\n                        <i class=\"ci-user opacity-60 me-2\"></i>\r\n                        Información personal\r\n                      </a>\r\n                    </li>\r\n                    <li class=\"border-top mb-0\">\r\n                      <a type=\"button\" class=\"nav-link-style d-flex align-items-center px-4 py-3\" @click=\"logout\">\r\n                        <i class=\"ci-sign-out opacity-60 me-2\"></i>\r\n                        Cerrar sesión\r\n                      </a>\r\n                    </li>\r\n                  </ul>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </aside>\r\n          <!-- Aquí NO se incluye la sección central de publicaciones -->\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport apiClient from \"@/services/ApiService\";\r\n\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      userData: {\r\n        name: \"\",\r\n        username: \"\",\r\n        description: \"\",\r\n        profile_picture: \"\",\r\n      },\r\n      countPosts: 0,\r\n      follows: {\r\n        followers: 0,\r\n        following: 0,\r\n      },\r\n      isCurrentUser: false,\r\n    };\r\n  },\r\n  methods: {\r\n    getUserImage() {\r\n      return (\r\n        this.userData.profile_picture ||\r\n        \"https://firebasestorage.googleapis.com/v0/b/booksharing-socialnetwork.appspot.com/o/profile%2Fdefault.jpg?alt=media\"\r\n      );\r\n    },\r\n    goToEditProfile() {\r\n      this.$router.push(`/profile/${this.userData.username}/edit-account`);\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"authToken\");\r\n      localStorage.removeItem(\"profileData\");\r\n      this.$router.push(\"/login\");\r\n    },\r\n    async fetchUserData() {\r\n      try {\r\n        const token = localStorage.getItem(\"authToken\");\r\n        if (!token) return;\r\n\r\n        // Puedes usar userId como prop o extraerlo de la ruta\r\n        const id = this.userId || this.$route.params.id;\r\n                const username = this.$route.params.username;\r\n\r\n\r\n        // Obtener datos de usuario\r\n        const resUser = await apiClient.get(`/user/${id}`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        this.userData = resUser.data;\r\n\r\n        // Obtener contadores\r\n        const resPosts = await apiClient.get(`/user/${id}/posts/count`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        this.countPosts = resPosts.data.count;\r\n\r\n        const resFollows = await apiClient.get(`/user/${id}/follows`, {\r\n          headers: { Authorization: `Bearer ${token}` },\r\n        });\r\n        this.follows = resFollows.data;\r\n\r\n        // Verificar si es el usuario actual\r\n        const profileData = JSON.parse(localStorage.getItem(\"profileData\"));\r\n        this.isCurrentUser = profileData && profileData._id === id;\r\n      } catch (error) {\r\n        console.error(\"Error al cargar datos de usuario:\", error);\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchUserData();\r\n  },\r\n};\r\n</script>"],"mappings":";AA+HA,OAAOA,SAAQ,MAAO,uBAAuB;AAE7C,eAAe;EAEbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,WAAW,EAAE,EAAE;QACfC,eAAe,EAAE;MACnB,CAAC;MACDC,UAAU,EAAE,CAAC;MACbC,OAAO,EAAE;QACPC,SAAS,EAAE,CAAC;QACZC,SAAS,EAAE;MACb,CAAC;MACDC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,OACE,IAAI,CAACX,QAAQ,CAACI,eAAc,IAC5B,qHAAoH;IAExH,CAAC;IACDQ,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAY,IAAI,CAACd,QAAQ,CAACE,QAAQ,eAAe,CAAC;IACtE,CAAC;IACDa,MAAMA,CAAA,EAAG;MACPC,YAAY,CAACC,UAAU,CAAC,WAAW,CAAC;MACpCD,YAAY,CAACC,UAAU,CAAC,aAAa,CAAC;MACtC,IAAI,CAACJ,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B,CAAC;IACD,MAAMI,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,KAAI,GAAIH,YAAY,CAACI,OAAO,CAAC,WAAW,CAAC;QAC/C,IAAI,CAACD,KAAK,EAAE;;QAEZ;QACA,MAAME,EAAC,GAAI,IAAI,CAACC,MAAK,IAAK,IAAI,CAACC,MAAM,CAACC,MAAM,CAACH,EAAE;QACvC,MAAMnB,QAAO,GAAI,IAAI,CAACqB,MAAM,CAACC,MAAM,CAACtB,QAAQ;;QAGpD;QACA,MAAMuB,OAAM,GAAI,MAAM3B,SAAS,CAAC4B,GAAG,CAAC,SAASL,EAAE,EAAE,EAAE;UACjDM,OAAO,EAAE;YA
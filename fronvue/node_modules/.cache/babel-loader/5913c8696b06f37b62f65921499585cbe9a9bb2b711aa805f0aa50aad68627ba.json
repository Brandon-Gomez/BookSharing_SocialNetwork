{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport apiClient from '../services/ApiService.js';\nexport default {\n  data() {\n    return {\n      email: '',\n      username: '',\n      password: ''\n    };\n  },\n  methods: {\n    async registerUser() {\n      try {\n        const response = await apiClient.post('/register', {\n          email: this.email,\n          username: this.username,\n          password: this.password\n        });\n        console.log('Registered:', response.data);\n        // Manejar el token o redirigir al usuario\n        localStorage.setItem('authToken', response.data.token);\n        this.$router.push('/login'); // Redirige al login después de registrarse\n      } catch (error) {\n        console.error('Error en el registro:', error.response.data);\n      }\n    }\n  }\n};","map":{"version":3,"names":["apiClient","data","email","username","password","methods","registerUser","response","post","console","log","localStorage","setItem","token","$router","push","error"],"sources":["C:\\DATA\\POW\\BookSharing_SocialNetwork\\fronvue\\src\\components\\register_component.vue"],"sourcesContent":["<template>\r\n  <div class=\"login fadeInDown\">\r\n    <div id=\"formContent\">\r\n      <div class=\"fadeIn first\">\r\n        <img src=\"@/assets/LogoRM.png\" id=\"icon\" alt=\"logo\" />\r\n      </div>\r\n      <form @submit.prevent=\"registerUser\">\r\n        <input type=\"email\" id=\"email\" class=\"fadeIn second\" placeholder=\"Email\" v-model=\"email\" required>\r\n        <input type=\"text\" id=\"username\" class=\"fadeIn third\" placeholder=\"Username\" v-model=\"username\" required>\r\n        <input type=\"password\" id=\"password\" class=\"fadeIn fourth\" placeholder=\"Password\" v-model=\"password\" required>\r\n        <input type=\"submit\" class=\"fadeIn fifth\" value=\"Register\">\r\n      </form>\r\n      <div id=\"formFooter\">\r\n        <router-link class=\"underlineHover\" to=\"/login\">¿Ya tienes una cuenta? Inicia sesión</router-link>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import apiClient from '../services/ApiService.js';\r\n\r\n  export default {\r\n    data() {\r\n      return {\r\n        email: '',\r\n        username: '',\r\n        password: '',\r\n      };\r\n    },\r\n    methods: {\r\n      async registerUser() {\r\n        try {\r\n          const response = await apiClient.post('/register', {\r\n            email: this.email,\r\n            username: this.username,\r\n            password: this.password\r\n          });\r\n          console.log('Registered:', response.data);\r\n          // Manejar el token o redirigir al usuario\r\n          localStorage.setItem('authToken', response.data.token);\r\n          this.$router.push('/login'); // Redirige al login después de registrarse\r\n        } catch (error) {\r\n          console.error('Error en el registro:', error.response.data);\r\n        }\r\n      }\r\n    }\r\n  };\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.register-form {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  background-color: #fff;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n  color: #333; /* Color para el texto del label */\r\n}\r\n\r\ninput {\r\n  width: 100%;\r\n  padding: 8px;\r\n  box-sizing: border-box;\r\n  border: 2px solid #ccc;\r\n  border-radius: 5px;\r\n  background-color: #f6f6f6; /* Color del fondo del input */\r\n  color: #0d0d0d; /* Color del texto en el input */\r\n}\r\n\r\ninput::placeholder {\r\n  color: #cccccc; /* Color del placeholder */\r\n}\r\n\r\nbutton {\r\n  background-color: #42b983; /* Color del botón */\r\n  color: white; /* Color del texto del botón */\r\n  border: none;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  text-transform: uppercase; /* Transformación del texto */\r\n  width: 100%; /* Asegura que el botón ocupe todo el ancho del formulario */\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #369f72; /* Color del botón al hacer hover */\r\n}\r\n\r\n.error-message {\r\n  color: red; /* Color para los mensajes de error */\r\n  margin-top: 5px;\r\n  font-size: 14px;\r\n  display: block;\r\n}\r\n\r\n.forgot-password {\r\n  margin-top: 10px;\r\n  text-align: right;\r\n}\r\n\r\n.forgot-password a {\r\n  color: #42b983; /* Color del enlace */\r\n  text-decoration: none;\r\n}\r\n\r\n.forgot-password a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n  .fadeInDown {\r\n    -webkit-animation-name: fadeInDown;\r\n    animation-name: fadeInDown;\r\n    -webkit-animation-duration: 1s;\r\n    animation-duration: 1s;\r\n    -webkit-animation-fill-mode: both;\r\n    animation-fill-mode: both;\r\n  }\r\n\r\n  @-webkit-keyframes fadeInDown {\r\n    0% {\r\n      opacity: 0;\r\n      -webkit-transform: translate3d(0, -100%, 0);\r\n      transform: translate3d(0, -100%, 0);\r\n    }\r\n    100% {\r\n      opacity: 1;\r\n      -webkit-transform: none;\r\n      transform: none;\r\n    }\r\n  }\r\n\r\n  @keyframes fadeInDown {\r\n    0% {\r\n      opacity: 0;\r\n      -webkit-transform: translate3d(0, -100%, 0);\r\n      transform: translate3d(0, -100%, 0);\r\n    }\r\n    100% {\r\n      opacity: 1;\r\n      -webkit-transform: none;\r\n      transform: none;\r\n    }\r\n  }\r\n\r\n  .fadeIn {\r\n    opacity: 0;\r\n    -webkit-animation: fadeIn ease-in 1;\r\n    -moz-animation: fadeIn ease-in 1;\r\n    animation: fadeIn ease-in 1;\r\n    -webkit-animation-fill-mode: forwards;\r\n    -moz-animation-fill-mode: forwards;\r\n    animation-fill-mode: forwards;\r\n    -webkit-animation-duration: 1s;\r\n    -moz-animation-duration: 1s;\r\n    animation-duration: 1s;\r\n  }\r\n\r\n  .fadeIn.first {\r\n    -webkit-animation-delay: 0.4s;\r\n    -moz-animation-delay: 0.4s;\r\n    animation-delay: 0.4s;\r\n  }\r\n\r\n  .fadeIn.second {\r\n    -webkit-animation-delay: 0.6s;\r\n    -moz-animation-delay: 0.6s;\r\n    animation-delay: 0.6s;\r\n  }\r\n\r\n  .fadeIn.third {\r\n    -webkit-animation-delay: 0.8s;\r\n    -moz-animation-delay: 0.8s;\r\n    animation-delay: 0.8s;\r\n  }\r\n\r\n  .fadeIn.fourth {\r\n    -webkit-animation-delay: 1s;\r\n    -moz-animation-delay: 1s;\r\n    animation-delay: 1s;\r\n  }\r\n</style>\r\n"],"mappings":";AAoBE,OAAOA,SAAQ,MAAO,2BAA2B;AAEjD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,SAAS,CAACQ,IAAI,CAAC,WAAW,EAAE;UACjDN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACFK,OAAO,CAACC,GAAG,CAAC,aAAa,EAAEH,QAAQ,CAACN,IAAI,CAAC;QACzC;QACAU,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEL,QAAQ,CAACN,IAAI,CAACY,KAAK,CAAC;QACtD,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC,EAAE;MAC/B,EAAE,OAAOC,KAAK,EAAE;QACdP,OAAO,CAACO,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAACT,QAAQ,CAACN,IAAI,CAAC;MAC7D;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}